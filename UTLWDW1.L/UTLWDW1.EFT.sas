%FILECHECK(12);

DATA LN83(drop=trunc);
      INFILE REPORT12 DSD DLM = ',' FIRSTOBS=1 MISSOVER end = eof;
      INPUT DF_SPE_ACC_ID :$10. LN_SEQ :6. BN_EFT_SEQ :6. LC_STA_LN83 :$1. 	;
