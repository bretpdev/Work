/*007 - FIRST DISBURSEMENT DATE UPPER BOUND*/
ELSE IF CRIT_NO = '007' THEN DO;
	CRIT_ARG = SUBSTR(CRIT_ARG,3,10);
	CRIT_ARG_DT = INPUT(CRIT_ARG,MMDDYY10.);
