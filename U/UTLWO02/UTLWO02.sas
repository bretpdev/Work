/*INTERPRET IMPORTED CRITERIA*/
DATA CRIT (DROP=CRIT_ARG_DT);
SET CRIT;
LENGTH CRIT_STM $10000;
/*000 - LOAN STATUS*/
IF CRIT_NO = '000' THEN DO;
	IF CRIT_OPR = 'EQ' 
		THEN CRIT_STM = 'WC_DW_LON_STA IN '||CRIT_ARG;
	ELSE IF CRIT_OPR = 'GT'
		THEN CRIT_STM = 'WC_DW_LON_STA NOT IN '||CRIT_ARG;
	ELSE IF CRIT_OPR = 'GE'
		THEN CRIT_STM = 'WC_DW_LON_STA IN '||CRIT_ARG;
