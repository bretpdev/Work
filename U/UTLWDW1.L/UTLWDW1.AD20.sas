%FILECHECK(36);

DATA AD20;
	INFILE REPORT36 DSD DLM = ',' FIRSTOBS=1 MISSOVER END = EOF;
	INPUT 
		DF_SPE_ACC_ID :$10.
		LD_FAT_ADJ_REQ :date9.
		LN_SEQ_FAT_ADJ_REQ 
		LC_TYP_FAT_ADJ_REQ :$2.
		LC_STA_FAT_ADJ_REQ :$1.
		
	;
	%TRUNCAT(36,DF_SPE_ACC_ID);
RUN;
%copyerror;

data ad20;
set ad20;
LD_FAT_ADJ_REQ = LD_FAT_ADJ_REQ * 86400;
run;

PROC SORT DATA=AD20; BY DF_SPE_ACC_ID; RUN;

%ENCHILADA
	(
		AD20,
		AD20_FinActAdjustment,
		DF_SPE_ACC_ID LD_FAT_ADJ_REQ LN_SEQ_FAT_ADJ_REQ LC_TYP_FAT_ADJ_REQ,
		LC_STA_FAT_ADJ_REQ,
	)
;

%FINISH;
