/*%LET RPTLIB = %SYSGET(reportdir);*/
%LET RPTLIB = T:\SAS;
FILENAME REPORTZ "&RPTLIB/ULWM43.LWM43RZ";
FILENAME REPORT2 "&RPTLIB/ULWM43.LWMR3R2";
LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;
LIBNAME DLGSUTWH DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
CREATE TABLE DEMO AS
SELECT COUNT(*)	AS LOANS
	,SUM(LA_CUR_PRI) AS LA_CUR_PRI
	,SUM(LA_NSI_OTS) AS LA_NSI_OTS
FROM DLGSUTWH.LN10_LON A
WHERE LD_LON_1_DSB >= '01OCT2007'D
	AND LA_CUR_PRI > 0
	AND LC_STA_LON10 = 'R'
	AND LF_LON_CUR_OWN IN ('828476','834529')
;
QUIT;
ENDRSUBMIT;
DATA DEMO;
	SET DUSTER.DEMO;
RUN;
PROC PRINTTO PRINT=REPORT2 NEW;
RUN;
/*FOR LANDSCAPE REPORTS:*/
OPTIONS ORIENTATION = LANDSCAPE;
OPTIONS PS=39 LS=127;
TITLE 'LOANS 1ST DISB ON OR AFTER 10-01-07';
FOOTNOTE4   'JOB = UTLWM43  	 REPORT = ULWM43.LWM43R2';

PROC PRINT NOOBS SPLIT='/' DATA=DEMO WIDTH=UNIFORM WIDTH=MIN LABEL;
FORMAT LOANS COMMA12. LA_CUR_PRI LA_NSI_OTS DOLLAR18.2 ;
VAR LOANS LA_CUR_PRI LA_NSI_OTS ;
LABEL LOANS = 'TOTAL NUMBER OF LOANS'
	LA_CUR_PRI = 'TOTAL CURRENT PRINCIPAL BALANCE'
	LA_NSI_OTS = 'TOTAL OUTSTANDING ACCRUED INTEREST';
RUN;

PROC PRINTTO;
RUN;
