
LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;
/*LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;*/

PROC SQL;
	CONNECT TO DB2 (DATABASE=DLGSUTWH);

	CREATE TABLE INITAL_POP AS
		SELECT 
			*
		FROM 
			CONNECTION TO DB2 
				(
					SELECT DISTINCT
						PD01.DF_PRS_ID	AS SSN,
						PD01.DF_SPE_ACC_ID AS ACCOUNT_NUMBER,
						PD01.DM_PRS_1 	AS FIRST,
						PD01.DM_PRS_LST AS LAST,
						LN10.LF_LON_ALT || LN10.LN_LON_ALT_SEQ AS UNIQUE_ID,
						RS10.LD_RPS_1_PAY_DU AS IBR_START_DATE	
					FROM	
						OLWHRM1.PD01_PDM_INF PD01
					INNER JOIN OLWHRM1.LN10_LON LN10
						ON LN10.BF_SSN = PD01.DF_PRS_ID
					INNER JOIN OLWHRM1.LN65_LON_RPS LN65
						ON LN65.BF_SSN = LN10.BF_SSN
						AND LN65.LN_SEQ = LN10.LN_SEQ
					INNER JOIN OLWHRM1.RS10_BR_RPD RS10
						ON RS10.BF_SSN = LN65.BF_SSN
						AND RS10.LN_RPS_SEQ = LN65.LN_RPS_SEQ
					WHERE 
						LN65.LC_TYP_SCH_DIS IN ('IB','IL')
						AND LN10.LC_STA_LON10 = 'R'
						AND DAYS(RS10.LD_RPS_1_PAY_DU) BETWEEN DAYS('01/01/2009') AND DAYS('12/31/2013')
				UNION ALL
					SELECT DISTINCT
						PD01.DF_PRS_ID	AS SSN,
						PD01.DF_SPE_ACC_ID AS ACCOUNT_NUMBER,
						PD01.DM_PRS_1 	AS FIRST,
						PD01.DM_PRS_LST AS LAST,
						LN10.LF_LON_ALT || LN10.LN_LON_ALT_SEQ AS UNIQUE_ID,
						RS10.LD_RPS_1_PAY_DU AS IBR_START_DATE	
					FROM	
						OLWHRM1.PD01_PDM_INF PD01
					INNER JOIN OLWHRM1.LN10_LON LN10
						ON LN10.BF_SSN = PD01.DF_PRS_ID
					INNER JOIN OLWHRM1.LN65_LON_RPS LN65
						ON LN65.BF_SSN = LN10.BF_SSN
						AND LN65.LN_SEQ = LN10.LN_SEQ
					INNER JOIN OLWHRM1.RS10_BR_RPD RS10
						ON RS10.BF_SSN = LN65.BF_SSN
						AND RS10.LN_RPS_SEQ = LN65.LN_RPS_SEQ
					WHERE 
						LN65.LC_TYP_SCH_DIS IN ('IB','IL')
						AND LN10.LC_STA_LON10 = 'R'
						AND DAYS(RS10.LD_STA_RPST10) BETWEEN DAYS('01/02/2009') AND DAYS('01/02/2010')

					FOR READ ONLY WITH UR
				)
	;

	DISCONNECT FROM DB2;

QUIT;
ENDRSUBMIT;

DATA FINAL; SET DUSTER.INITAL_POP; RUN;
PROC SORT DATA=FINAL NODUPKEY; BY SSN ACCOUNT_NUMBER FIRST LAST UNIQUE_ID IBR_START_DATE; RUN;


PROC EXPORT DATA= FINAL 
            OUTFILE= "T:\SAS\IBR Audit Query.xlsx" 
            DBMS=EXCEL REPLACE;
     SHEET="A"; 
RUN;
