/* Dividing a dataset PCANC into 2 subsets PCANC1 & PCANC2
so the data can be exported to 2 Excel files */

DATA PCANC1 PCANC2;
DO SUBSET = 1 TO 35000;
	SET PCANC POINT = SUBSET;
	OUTPUT PCANC1;
END;
DO SUBSET = 35001 TO 67349;
	SET PCANC POINT = SUBSET;
	OUTPUT PCANC2;
END;
STOP;
RUN;


DATA LDRDAT_2_1;
SET WORK.LDRDAT_2 (FIRSTOBS=1 OBS=60000);
run;
DATA LDRDAT_2_2;
SET WORK.LDRDAT_2 (FIRSTOBS=60001 OBS=MAX);
run;