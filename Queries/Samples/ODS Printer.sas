
ODS LISTING CLOSE;
ODS PRINTER style=statdoc fontscale=75;
OPTIONS CENTER NODATE NUMBER LS=145 PS=60 pageno=1;
PROC REPORT DATA=CONSOL NOWD SPACING=2 HEADSKIP SPLIT='/'
/*	style(header)=[font_size=2]
	STYLE(COLUMN)=[font_face=Helvetica font_size=1]*/
	style(lines)=[font_face="Lucida Console" font_size=2 just=l];
TITLE 'Consolidated Loans that Re-entered Default';
TITLE2 "Run Date:  &sysdate9";
FOOTNOTE  'JOB = ON DEMAND     REPORT = Consols that Re-enter Default.sas';
COLUMN SSN CLUID INITSTA INITBAL INITAUXDT SUBSPOF MOS
	SUBSBAL SUBSSTA SUBSAUX CURBAL DIFF DIFFPRN N SORT COUNT AVGMOS AVGDIFF BWRS
	BWRNO LOANNO BWRNUM LOANNUM;
DEFINE SSN / DISPLAY FORMAT=SSN11.;
DEFINE CLUID / DISPLAY 'Commonline/Unique/ID';
DEFINE INITSTA / DISPLAY 'Initial/Loan/Status' WIDTH=7;
DEFINE INITBAL / DISPLAY FORMAT=DOLLAR12.2 'Initial/Balance/at/Claim Pmt';
DEFINE INITAUXDT / DISPLAY FORMAT=MMDDYY8. 'Initial/Aux/Status/Date';
DEFINE SUBSPOF / DISPLAY FORMAT=MMDDYY8. 'Subseq/Lender/Payoff';
DEFINE MOS / ANALYSIS MEAN 'Months/to/Re-enter/Default' WIDTH=9;
DEFINE SUBSBAL / DISPLAY FORMAT=DOLLAR12.2 'Subseq/Balance/at/Claim Pmt' ;
DEFINE SUBSSTA / DISPLAY 'Subseq/DC10/Status' WIDTH=6;
DEFINE SUBSAUX / DISPLAY 'Subseq/Aux/Status' WIDTH=6;
DEFINE CURBAL / DISPLAY FORMAT=DOLLAR12.2 'Current/Claim/Balance';
DEFINE DIFF / NOPRINT ANALYSIS MEAN FORMAT=DOLLAR12.2;
DEFINE DIFFPRN / DISPLAY 'Difference/in/Balance' RIGHT;
DEFINE N / NOPRINT WIDTH=5;
DEFINE SORT / NOPRINT ORDER DESCENDING;
DEFINE COUNT / NOPRINT ;
DEFINE AVGMOS / NOPRINT COMPUTED FORMAT=3.;
DEFINE AVGDIFF / NOPRINT COMPUTED;
DEFINE BWRS / NOPRINT COMPUTED;
DEFINE BWRNO / NOPRINT ANALYSIS MEAN;
DEFINE LOANNO / NOPRINT ANALYSIS MEAN;
DEFINE BWRNUM / NOPRINT COMPUTED;
DEFINE LOANNUM / NOPRINT COMPUTED;

COMPUTE BWRS;
	BWRS = COUNT.SUM;
ENDCOMP;
COMPUTE AVGMOS;
	AVGMOS = MOS.MEAN;
ENDCOMP;
COMPUTE AVGDIFF;
	AVGDIFF = DIFF.MEAN;
ENDCOMP;
COMPUTE BWRNUM;
	BWRNUM = BWRNO.MEAN;
ENDCOMP;
COMPUTE LOANNUM;
	LOANNUM = LOANNO.MEAN;
ENDCOMP;

COMPUTE AFTER SORT;
*	LINE ' ';
	LINE @1 "# BORROWERS, THIS TYPE:  " @25 BWRS COMMA6.;
*	LINE ' ';
ENDCOMP;

COMPUTE AFTER;
*	LINE ' ';
	LINE ' ';
	LINE @1 "NUMBER OF BORROWERS:  " @50 BWRS COMMA6.;
	LINE @1 "NUMBER OF LOANS:  " @50 N COMMA6.;
	LINE @1 "AVG # OF MONTHS TO RE-ENTER DEFAULT:  " @52 AVGMOS 4.1;
	LINE @1 "AVERAGE DIFFERENCE IN BALANCE:" @44 AVGDIFF DOLLAR12.2;
	LINE @1 ' ';
	LINE @1 "TOTAL BORROWERS WITH YEAR-OLD CONSOLS:" @49 BWRNUM COMMA7.;
	LINE @1 "TOTAL YEAR-OLD CONSOL LOAN COUNT:" @49 LOANNUM COMMA7.;
ENDCOMP;
RUN;
ODS PRINTER CLOSE;
ODS LISTING;

DATA _NULL_;
TITLE;
FOOTNOTE;
FILE PRINT;
PUT "'Consolidated Loans that Re-entered Default' has been sent to your default printer.";
RUN;