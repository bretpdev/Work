*FINDING DUPLICATES IN A DATASET;

PROC SQL;
CREATE TABLE XXX AS
SELECT CLUID, COUNT(*) AS COUNT
FROM FULL
GROUP BY CLUID
HAVING COUNT(*) > 1;
QUIT;

*DELETING DUPLICATES CONDITIONALLY (more flexible than PROC SORT NODUP%;

PROC SQL;
DELETE FROM FULL A
WHERE A.bd_leg_act_aty_fil = '31MAY2001'D	/*PROC SORT can't do this.*/
AND A.lf_crt_dts_dc10 IN (SELECT lf_crt_dts_dc10
						FROM FULL
						GROUP BY lf_crt_dts_dc10
						HAVING COUNT(*) > 1
					 	 );
QUIT;