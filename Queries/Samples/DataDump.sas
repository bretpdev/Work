%LET RPTLIB = T:\SAS;
FILENAME REPORT2 "&RPTLIB/QQoutput";
LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=work  ;

RSUBMIT LEGEND;
PROC SQL;
CONNECT TO DB2 (DATABASE=DNFPUTDL);/*DNFPUTDL, DNFPRQUT*/
CREATE TABLE XEMO AS
SELECT *
FROM CONNECTION TO DB2 (
SELECT *
FROM WEBFLS1.WB24_CSM_USR_ACC 
FOR READ ONLY WITH UR
);
CREATE TABLE XEMO2 AS
SELECT *
FROM CONNECTION TO DB2 (
SELECT *
FROM WEBFLS1.RM03_ONL_PAY
WHERE LD_PAY > '12/31/2015'
FOR READ ONLY WITH UR
);
CREATE TABLE XEMO3 AS
SELECT *
FROM CONNECTION TO DB2 (
SELECT *
FROM WEBFLS1.RM12_OPS_BIL_MTD 
FOR READ ONLY WITH UR
);
DISCONNECT FROM DB2;
QUIT;
ENDRSUBMIT;

DATA XEMO;
	SET LEGEND.XEMO;
RUN;
DATA XEMO2;
	SET LEGEND.XEMO2;
RUN;
DATA XEMO3;
	SET LEGEND.XEMO3;
RUN;

PROC EXPORT DATA= XEMO
            OUTFILE= "T:\SAS\DataDump.&sysdate..xlsx" 
            DBMS=EXCEL REPLACE;
			SHEET="WB24";
RUN;
PROC EXPORT DATA= XEMO2
            OUTFILE= "T:\SAS\DataDump.&sysdate..xlsx" 
            DBMS=EXCEL REPLACE;
			SHEET="RM03";
RUN;
PROC EXPORT DATA= XEMO3
            OUTFILE= "T:\SAS\DataDump.&sysdate..xlsx" 
            DBMS=EXCEL REPLACE;
			SHEET="RM12";
RUN;
