LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DB2 DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE LITS AS
		SELECT
			LN10.BF_SSN,
			CATX('',FS10.LF_FED_AWD,PUT(FS10.LN_FED_AWD_SEQ,Z3.)) AS AWARD_ID,
			LN09.IF_LON_SRV_DFL_LON
		FROM
			PKUB.LN10_LON LN10
			JOIN PKUB.FS10_DL_LON FS10
				ON LN10.BF_SSN = FS10.BF_SSN
				AND LN10.LN_SEQ = FS10.LN_SEQ
			JOIN PKUB.LN09_RPD_PIO_CVN LN09
				ON LN10.BF_SSN = LN09.BF_SSN
				AND LN10.LN_SEQ = LN09.LN_SEQ
		WHERE
			LN10.LC_STA_LON10 = 'L'
	;
QUIT;

ENDRSUBMIT;

DATA LITS; SET LEGEND.LITS; RUN;

PROC EXPORT
		DATA=LITS
		OUTFILE='T:\SAS\Redefaulted Loans.XLSX'
		DBMS=XLSX
		REPLACE;
RUN;
