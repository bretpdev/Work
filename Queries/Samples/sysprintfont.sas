
filename myfile printer;
OPTIONS NOCENTER NODATE NONUMBER LS=200 PS=60 options 
OBS=15 sysprintfont="SAS Monospace" 8;
DATA _NULL_;
SET LDRDAT;
*FILE PRINT FOOTNOTES;
FILE MYFILE FOOTNOTES;
TITLE "LENDER VOLUME REPORT (Borrower Level)";
TITLE2 "PERIOD &BEGIN TO &END";
BY LENDER SSN;
IF FIRST.LENDER THEN DO;
	PUT @1 'LENDER:  'LENDER $29.;
	PUT ' ';
END;
IF FIRST.SSN THEN DO;
	PUT @1 SSN SSN11. @14 NAME $25. @41 'STUDENT ENROLLMENT STATUS:  ' STUENR $1.;
END;
PUT @19 SRVCD @27 CLUID @48 LONTYP @52 GTYDT MMDDYY8. @62 GTYAMT COMMA8.2 @72 INTTYP 
@75 TRMBEG MMDDYY8. @85 TRMEND MMDDYY8. @95 LONSTA 
@99 '1' @102 DISB1DT MMDDYY8. @112 DISB1AMT COMMA8.2 
@122 AGD MMDDYY8. @132 SCH_OPE @142 REFAMT COMMA8.2 @152 CANAMT COMMA8.2 ;
PUT @99 '2' @102 DISB2DT MMDDYY8. @112 DISB2AMT COMMA8.2;
PUT @99 '3' @102 DISB3DT MMDDYY8. @112 DISB3AMT COMMA8.2;
PUT @99 '4' @102 DISB4DT MMDDYY8. @112 DISB4AMT COMMA8.2;
PUT @112 '--------';
PUT @112 TOTDISB COMMA8.2;
PUT ' ';
IF LAST.LENDER THEN DO;
	PUT ' ';
	PUT 'TOTAL NUMBER OF REFERENCES:  ' @30 _N_;
END;
FOOTNOTE1  ' ';
FOOTNOTE2  'JOB = QUERY     REPORT = School & Lender Volume (48)';
RUN;
options sysprintfont="SAS Monospace" 10;
