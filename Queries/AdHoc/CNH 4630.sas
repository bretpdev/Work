LIBNAME EX 'T:\Xerox-Cornerstone Scrub Xnd round.xlsx';
LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;

DATA LEGEND.ELEC;
	SET EX.'ELEC$'N;
RUN;

RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE ELEC_ AS
		SELECT DISTINCT
			ELEC.*,
			PDXX.DF_SPE_ACC_ID,
			LNXX.LC_STA_LONXX
		FROM
			ELEC
			JOIN PKUB.PDXX_PRS_NME PDXX
				ON ELEC.SSN = PDXX.DF_PRS_ID
			JOIN PKUB.LNXX_LON LNXX
				ON ELEC.SSN = LNXX.BF_SSN
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA = LEGEND.ELEC_
		OUTFILE='T:\SASR XXXX.XLSX'
		REPLACE;
RUN;
