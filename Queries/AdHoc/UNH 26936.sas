

PROC IMPORT OUT= WORK.SOURCE
            DATAFILE= "T:\DCR U20160318113431.xlsx" 
            DBMS=EXCEL REPLACE;
     RANGE="SHEET1$"; 
     GETNAMES=YES;
     MIXED=NO;
     SCANTEXT=YES;
     USEDATE=YES;
     SCANTIME=YES;
RUN;


LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;

DATA DUSTER.SOURCE;
SET SOURCE;
RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE RS10 AS
		SELECT DISTINCT
			RS10.*
		FROM
			OLWHRM1.RS10_BR_RPD RS10
		INNER JOIN SOURCE S
			ON S.BF_SSN = RS10.BF_SSN
		WHERE
			RS10.LC_STA_RPST10 = 'A'
	;

	CREATE TABLE RS05 AS
		SELECT DISTINCT
			RS05.*
		FROM
			OLWHRM1.RS05_IBR_RPS RS05
		INNER JOIN SOURCE S
			ON S.BF_SSN = RS05.BF_SSN
		WHERE
			RS05.BC_STA_RS05 = 'A'

	;

	CREATE TABLE PLUG AS
		SELECT DISTINCT
			RS10.BF_SSN,
			RS10.LN_RPS_SEQ,
			RS10.BD_CRT_RS05 AS OLD_BD_CRT_RS05,
			RS10.BN_IBR_SEQ AS OLD_BN_IBR_SEQ,
			S.BD_CRT_RS05 AS NEW_BD_CRT_RS05,
			S.BN_IBR_SEQ AS NEW_BN_IBR_SEQ
		FROM
			OLWHRM1.RS10_BR_RPD RS10
		INNER JOIN SOURCE S
			ON S.BF_SSN = RS10.BF_SSN
		WHERE
			RS10.LC_STA_RPST10 = 'A'
	;
QUIT;

ENDRSUBMIT;

DATA RS10;
SET DUSTER.RS10;
RUN;

DATA RS05;
SET DUSTER.RS05;
RUN;

DATA PLUG;
SET DUSTER.PLUG;
RUN;



PROC EXPORT DATA = WORK.RS10 
            OUTFILE = "T:\NH 26936.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="RS10"; 
RUN;

PROC EXPORT DATA = WORK.RS05 
            OUTFILE = "T:\NH 26936.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="RS05"; 
RUN;

PROC EXPORT DATA = WORK.PLUG 
            OUTFILE = "T:\NH 26936.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="PLUG"; 
RUN;


