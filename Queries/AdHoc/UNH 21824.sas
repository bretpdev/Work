LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
LIBNAME XL 'T:\Need LN56 table dump.xlsx';

DATA SSNS;
	SET XL.'LN56$'N;
RUN;

DATA DUSTER.SSNS; SET SSNS; RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LN56 AS
		SELECT DISTINCT
			LN56.*
		FROM
			SSNS
			JOIN OLWHRM1.LN56_LON_CRB_RPT LN56
				ON SSNS.BF_SSN = LN56.BF_SSN
				AND SSNS.LN_SEQ = LN56.LN_SEQ
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LN56
		OUTFILE='T:\LN56 DATA DUMP.XLSX'
		REPLACE;
RUN;
