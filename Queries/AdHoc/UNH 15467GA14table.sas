/*%LET RPTLIB = %SYSGET(reportdir);*/
%LET RPTLIB = T:\SAS;

LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;


/*Note the GA14.AF_APL_ID and or GA14.AF_APL_ID_SFX may need to be changed depends on what they want...*/
PROC SQL;
CONNECT TO DB2 (DATABASE=DLGSUTWH);
CREATE TABLE DEMO AS
SELECT *
FROM 
	CONNECTION TO DB2 (
		SELECT
			GA14.AF_APL_ID,
			GA14.AF_APL_ID_SFX,
			GA14.AF_LON_STA_RPT_SEQ,
			GA14.AF_CRT_DTS_GA14

		FROM	OLWHRM1.GA14_LON_STA GA14
		WHERE	GA14.AF_APL_ID 
				IN (
					'0036770000B022041','0036770000B033220',
					'00367500000033917','00367500000047490',
					'00367500000050903'
				   )
		AND GA14.AF_APL_ID_SFX = '01'

		FOR READ ONLY WITH UR
						);
DISCONNECT FROM DB2;



ENDRSUBMIT;
DATA DEMO;
	SET DUSTER.DEMO;
RUN;

PROC EXPORT DATA= WORK.DEMO 
            OUTFILE= "T:\SAS\GA14_Query.xlsz" 
            DBMS=EXCEL REPLACE;
     SHEET="A"; 
RUN;
