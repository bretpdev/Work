LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE QUEKILL AS
		SELECT
			WF_QUE,
			WF_SUB_QUE,
			WN_CTL_TSK,
			PF_REQ_ACT
		FROM
			PKUB.WQXX_TSK_QUE WQXX
		WHERE
			WF_QUE = 'PU'
	;
QUIT;
ENDRSUBMIT;

DATA QUEKILL; SET LEGEND.QUEKILL; RUN;

PROC EXPORT
		DATA=QUEKILL
		OUTFILE='T:\QUEKILL.XLSX'
		REPLACE;
RUN;
