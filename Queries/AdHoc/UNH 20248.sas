LIBNAME XL 'T:\LN10-HP 20248.xlsx';
LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;

DATA DUSTER.LN10_HP; SET XL.'LN10$'N; RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE LN10C AS
		SELECT
			HP.BF_SSN,
			HP.LN_SEQ,
			LN10.LD_EFF_LBR_RTE AS LD_EFF_LBR_RTE__CURRENT,
			HP.LD_EFF_LBR_RTE__UPDATED_
		FROM
			LN10_HP HP
			JOIN OLWHRM1.LN10_LON LN10
				ON HP.BF_SSN = LN10.BF_SSN
				AND HP.LN_SEQ = LN10.LN_SEQ
	;
QUIT;

ENDRSUBMIT;

DATA LN10C; SET DUSTER.LN10C; RUN;

PROC EXPORT
		DATA=LN10C
		OUTFILE='T:\SAS\LIBOR.XLSX'
		REPLACE;
RUN;
