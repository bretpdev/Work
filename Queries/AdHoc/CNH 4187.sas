
PROC IMPORT OUT = WORK.SOURCE
            DATAFILE = "T:\FISERV Scrub XXXXXX.xlsx" 
            DBMS = xlsx REPLACE;
   			SHEET = 'SheetX'; 
RUN;


LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.SOURCE; *Send data to Duster;
SET SOURCE;
RUN;

RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT DISTINCT
			SOURCE.*,
			PDXX.DF_SPE_ACC_ID AS ACCOUNT_NUMBER,
			PDXX.DM_PRS_X || ' ' || PDXX.DM_PRS_LST AS NAME,
			LNXX.LC_STA_LONXX AS BORROWER_STATUS
		FROM
			SOURCE S
		INNER JOIN PKUB.LNXX_LON LNXX
			ON LNXX.BF_SSN = S.SSN
		INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_PRS_ID = LNXX.BF_SSN

;
QUIT;
ENDRSUBMIT;

DATA NHXXXX; SET LEGEND.NHXXXX; RUN;

DATA DECONVERTED;
SET NHXXXX;
WHERE BORROWER_STATUS = 'D';
RUN;

PROC EXPORT DATA = WORK.NHXXXX 
            OUTFILE = "T:\SAS\NHXXXX.xls" 
            DBMS = EXCEL
			REPLACE;
     SHEET="SheetX"; 
RUN;

PROC EXPORT DATA = WORK.DECONVERTED 
            OUTFILE = "T:\SAS\NHXXXX.xls" 
            DBMS = EXCEL
			REPLACE;
     SHEET="SheetX"; 
RUN;

