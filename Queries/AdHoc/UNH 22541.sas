LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
LIBNAME XL 'T:\22541 Data Dump.xlsx';

DATA DUSTER.RECS; SET XL.'SHEET1$'N; RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LN10 AS
		SELECT
			*
		FROM
			OLWHRM1.LN10_LON LN10
			JOIN RECS
				ON LN10.BF_SSN = PUT(RECS.SSN,Z9.)
				AND LN10.LN_SEQ = RECS.LOAN_SEQ
	;
QUIT;

PROC SQL;
	CREATE TABLE LN83 AS
		SELECT
			*
		FROM
			OLWHRM1.LN83_EFT_TO_LON LN83
			JOIN RECS
				ON LN83.BF_SSN = PUT(RECS.SSN,Z9.)
				AND LN83.LN_SEQ = RECS.LOAN_SEQ
	;
QUIT;
ENDRSUBMIT;

DATA LN10; SET DUSTER.LN10; RUN;
DATA LN83; SET DUSTER.LN83; RUN;

PROC EXPORT
		DATA=LN10
		OUTFILE='T:\DATA DUMP NH 22541.XLSX'
		REPLACE;
RUN;

PROC EXPORT
		DATA=LN83
		OUTFILE='T:\DATA DUMP NH 22541.XLSX'
		REPLACE;
RUN;
