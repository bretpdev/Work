LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE CRIL AS
		SELECT DISTINCT 
			LN35.IF_OWN,
			COUNT(DISTINCT CAT(LN10.BF_SSN,PUT(LN10.LN_SEQ,Z4.))) AS LOANS
/*			CAT(LN10.BF_SSN,PUT(LN10.LN_SEQ,Z4.)) AS LOANS*/
		FROM
			OLWHRM1.LN10_LON_EOM LN10
			JOIN OLWHRM1.LN35_LON_OWN LN35
				ON LN10.BF_SSN = LN35.BF_SSN
				AND LN10.LN_SEQ = LN35.LN_SEQ
		WHERE
			LN35.IF_OWN IN ('826717','827283','828476','830248')
			AND LN10.LA_CUR_PRI > 0
			AND LN10.LC_STA_LON10 = 'R'
			AND LN35.LD_OWN_EFF_SR < '01JAN2015'D
		GROUP BY
			LN35.IF_OWN
	;
QUIT;
ENDRSUBMIT;
