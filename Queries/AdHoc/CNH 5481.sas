RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE GROUP_X AS
		SELECT DISTINCT
			LNXX.BF_SSN,
			LNXX.LN_SEQ,
			LNXX.LA_CUR_PRI,
			DFXX.LC_DFR_TYP,
			DFXX.LD_DFR_REQ_BEG FORMAT MMDDYYXX.,
			DFXX.LD_DFR_REQ_END FORMAT MMDDYYXX.
		FROM
			PKUB.LNXX_LON LNXX
			INNER JOIN PKUB.LNXX_BR_DFR_APV LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN
				AND LNXX.LN_SEQ = LNXX.LN_SEQ
				AND LNXX.LC_STA_LONXX = 'A'
			INNER JOIN PKUB.DFXX_BR_DFR_REQ DFXX
				ON DFXX.BF_SSN = LNXX.BF_SSN
				AND DFXX.LF_DFR_CTL_NUM = LNXX.LF_DFR_CTL_NUM
				AND DFXX.LC_DFR_STA = 'A'
				AND DFXX.LC_STA_DFRXX = 'A'
		WHERE
			LNXX.LA_CUR_PRI > X
			AND LNXX.LC_STA_LONXX = 'R'
			AND DFXX.LC_DFR_TYP IN ('XX','XX')
		ORDER BY
			LNXX.BF_SSN,
			LNXX.LN_SEQ
;
	PROC SQL;
		CREATE TABLE GROUP_X AS
			SELECT DISTINCT
				*
			FROM
				GROUP_X
			WHERE
				LD_DFR_REQ_END > TODAY()
;
QUIT;
ENDRSUBMIT;

DATA GROUP_X; SET LEGEND.GROUP_X; RUN;
DATA GROUP_X; SET LEGEND.GROUP_X; RUN;

PROC EXPORT DATA= WORK.GROUP_X 
            OUTFILE= "T:\NH XXXX.xlsx" 
            DBMS=EXCEL REPLACE;
     SHEET="XXXX"; 
RUN;
