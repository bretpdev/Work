


RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE BWR_POP AS
		SELECT DISTINCT
			LNXX.BF_SSN AS SSN,
			LNXX.LN_SEQ AS LOAN_SEQ, 
			LNXX.LD_FAT_APL AS TRANSACTION_DATE
		FROM
			PKUB.LNXX_FIN_ATY LNXX
			
		WHERE
			LNXX.PC_FAT_TYP = 'XX'
			AND LNXX.PC_FAT_SUB_TYP = 'XX'
			AND LNXX.LD_FAT_APL BETWEEN (INPUT('XX/XX/XXXX', MMddyyXX.)) AND TODAY()
;
QUIT;



PROC SQL;
	CREATE TABLE NEW_LOANS AS
		SELECT DISTINCT
			LNXX_DATA.BF_SSN,
			LNXX_DATA.LN_SEQ,
			LNXX_DATA.LD_LON_EFF_ADD AS LOAN_ADD_DATE
		FROM
			BWR_POP BWRPOP
		INNER JOIN
		(
			SELECT DISTINCT 
				BF_SSN,
				LN_SEQ,
				LD_LON_EFF_ADD
			FROM PKUB.LNXX_LON
		) LNXX_DATA
		ON LNXX_DATA.BF_SSN = BWRPOP.SSN
		AND LNXX_DATA.LD_LON_EFF_ADD >= BWRPOP.TRANSACTION_DATE

;
QUIT;
ENDRSUBMIT;


PROC EXPORT DATA = LEGEND.BWR_POP 
            OUTFILE = "T:\SAS\NHXXXX.xls" 
            DBMS = EXCEL
			REPLACE;
     SHEET="BWR_POP"; 
RUN;

PROC EXPORT DATA = LEGEND.NEW_LOANS 
            OUTFILE = "T:\SAS\NHXXXX.xls" 
            DBMS = EXCEL
			REPLACE;
     SHEET="NEW_LOANS"; 
RUN;

