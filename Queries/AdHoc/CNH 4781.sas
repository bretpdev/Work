LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE RPXX AS
		SELECT
			IF_OWN,
			PN_EFT_RIR_OWN_SEQ,
			PC_EFT_BBS_NSF_PRC,
			PI_EFT_RIR_PRC
		FROM
			PKUB.RPXX_EFT_RIR_PAR
		WHERE
			PC_EFT_BBS_NSF_PRC = 'C'
			AND PI_EFT_RIR_PRC = 'Y'
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.RPXX
		OUTFILE='T:\SAS\NH XXXX.xlsx'
		REPLACE;
RUN;
