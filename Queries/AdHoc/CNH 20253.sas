LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

%LET BEGIN = 'XXJULXXXX'D;

PROC SQL;
	CREATE TABLE DEFFOR AS
		SELECT
			BF_SSN,
			PF_REQ_ACT,
			LD_ATY_REQ_RCV
		FROM
			PKUB.AYXX_BR_LON_ATY
		WHERE
			PF_REQ_ACT IN ('BRRPF','XFORB','DDFRB','DIFRB','EHFRB','FVRXX','GVFRB','GXXXB','GXXXD','HDFRB','IRFRB','LDBFB','LSXXX','SCFRB','TFRFB','VBFRB','WRFXX','WRFXX','WRFXX','DIDFR','EMDFR','LSXXX','LSXXX','LSXXX','LSXXX','LSXXX','LSXXX','LSXXX','WRDXX','TBRDG')
			AND LD_ATY_REQ_RCV >= &BEGIN
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT DATA = LEGEND.DEFFOR
            OUTFILE = "T:\SSAE XX Audit - Def-Forb Received_XX-X-XXXX.xlsx"
            DBMS = EXCEL
			REPLACE;
     SHEET="Results"; 
RUN;
