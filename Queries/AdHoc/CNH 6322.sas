LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

DATA LNXX;
	SET PKUB.LNXX_LON;
	WHERE LD_LON_ACL_ADD >= 'XXDECXXXX'D;
RUN;

ENDRSUBMIT;

DATA LNXX; SET LEGEND.LNXX; RUN;

LIBNAME PKUB 'Y:\Development\ExternalDataSources\MonthlyDataSets';

PROC SQL;
	CREATE TABLE DWXX AS
		SELECT
			LNXX.BF_SSN AS LNXX_BF_SSN,
			LNXX.LN_SEQ AS LNXX_LN_SEQ,
			LNXX.IC_LON_PGM,
			DWXX.*
		FROM
			LNXX
			LEFT JOIN PKUB.Pkub_dwXX_dw_clc_clu_XXXXXX DWXX
				ON DWXX.BF_SSN = LNXX.BF_SSN
				AND DWXX.LN_SEQ = LNXX.LN_SEQ
	;
QUIT;

PROC EXPORT
		DATA=DWXX
		OUTFILE='T:\DWXX for loans added December XXXX.XLSX'
		REPLACE;
RUN;
		
