LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE BLXX_ AS
		SELECT DISTINCT
			BLXX.BF_SSN,
/*			LNXX.LN_SEQ,*/
			BLXX.LD_BIL_DU,
			BLXX.LC_IND_BIL_SNT,
			BLXX.LD_BIL_CRT
		FROM
			PKUB.BLXX_BR_BIL BLXX
			JOIN PKUB.LNXX_LON LNXX
				ON BLXX.BF_SSN = LNXX.BF_SSN
		WHERE 
			BLXX.LC_IND_BIL_SNT NOT IN ('X', 'X', 'X', 'X', 'F', 'G', 'C', 'T')
			AND BLXX.LD_BIL_CRT > 'XXNOVXXXX'D
			AND BLXX.LC_STA_BILXX = 'A'
			AND LNXX.LC_LON_SND_CHC = 'Y'
	;
QUIT;

ENDRSUBMIT;

DATA BLXX; SET LEGEND.BLXX_; RUN;

PROC EXPORT
		DATA=BLXX
		OUTFILE='T:\SAS\NH XXXX BLXX.XLSX'
		REPLACE;
RUN;
