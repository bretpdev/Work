              / @XX " ********************************************************************* "
            ;
         RUN  ;
  %END  ;
%MEND  ;

%MACRO TBL_PUL(DSET,TABLE);
PROC SQL ;
CONNECT TO DBX (DATABASE=&DB);
CREATE TABLE &DSET AS
SELECT *
FROM CONNECTION TO DBX (
	SELECT DISTINCT
		A.*
	FROM
		PKUB.&TABLE A
	WHERE 
		A.IC_LON_PGM IN ('DLSTFD','DLUNST','DLPLUS','DLPLGB','SLS','DLSCNS','DLUCNS','CNSLDN')

	FOR READ ONLY WITH UR
				)
	;

DISCONNECT FROM DBX;
%MEND TBL_PUL;
