LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
%let DB = DNFPUTDL;  *This is live;

PROC SQL;
	CONNECT TO DBX (DATABASE=&DB);

	CREATE TABLE FBXX AS
		SELECT	
			*
		FROM	
			CONNECTION TO DBX 
	(
		SELECT 
			PDXX.DF_SPE_ACC_ID
		FROM
			PKUB.PDXX_PRS_NME PDXX
			INNER JOIN PKUB.FBXX_BR_FOR_REQ FBXX
			ON PDXX.DF_PRS_ID = FBXX.BF_SSN
		WHERE 
			FBXX.LC_FOR_TYP = 'XX'
			AND FBXX.LI_CAP_FOR_INT_REQ = 'N'
			AND FBXX.LC_STA_FORXX = 'A'
		FOR READ ONLY WITH UR
	)
	;
	DISCONNECT FROM DBX;
QUIT;

ENDRSUBMIT;

DATA FBXX; SET LEGEND.FBXX; RUN;

PROC EXPORT	
		DATA=LEGEND.FBXX
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;
