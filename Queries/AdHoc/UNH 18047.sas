LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;

PROC IMPORT
		DATAFILE='T:\SAS\UHEAA FY14 Commercial Servicer IBR Sample.xls'
		OUT=IBR
		DBMS=EXCEL
		REPLACE;
RUN;

DATA DUSTER.IBR (DROP=F10);
	SET IBR;
RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE ANNIV AS
		SELECT
			IBR.*,
			RS05.BD_ANV_QLF_IBR,
			RS05.BC_STA_RS05
		FROM
			IBR
			JOIN OLWHRM1.RS05_IBR_RPS RS05
				ON IBR.SSN = RS05.BF_SSN
		ORDER BY
			IBR.SAMPLE_NUMBER
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.ANNIV
		OUTFILE='T:\SAS\IBR Anniversary Date.xlsx'
		REPLACE;
RUN;



