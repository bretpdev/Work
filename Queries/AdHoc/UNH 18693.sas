LIBNAME AC10ZZ 'T:\AC10_ZZ.XLSX';

LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;

DATA DUSTER.AC10ZZ (KEEP=PF_REQ_ACT);
	SET AC10ZZ.AC10ZZ;
RUN;

RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE AC10 AS
		SELECT
			AC10.*
		FROM
			AC10ZZ ZZ
			JOIN OLWHRM1.AC10_ACT_REQ AC10
				ON ZZ.PF_REQ_ACT = AC10.PF_REQ_ACT
			LEFT JOIN OLWHRM1.LP15_DD_ACT_STE LP15
				ON ZZ.PF_REQ_ACT = LP15.PF_REQ_ACT
				AND LP15.PC_STA_LPD13 = 'A'
				AND LP15.IF_GTR = '000749'
				AND LP15.IF_OWN = '828476'
		WHERE
			LP15.PF_REQ_ACT IS NULL
	;
QUIT;

ENDRSUBMIT;

DATA AC10; SET DUSTER.AC10; RUN;

PROC EXPORT
		DATA=AC10
		OUTFILE='T:\AC10 DATA DUMP.XLSX'
		REPLACE;
RUN;
