LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE LPXX AS
		SELECT
			LPXX.*
		FROM
			PKUB.LPXX_DFR_PAR LPXX
		WHERE
			LPXX.PC_MAX_CUM_GRP_DFR <> ''
	;

	CREATE TABLE LPXX AS
		SELECT
			LPXX.*
		FROM
			PKUB.LPXX_FOR_PAR LPXX
		WHERE
			LPXX.PC_MAX_CUM_GRP_FOR <> ''
	;
QUIT;
ENDRSUBMIT;

DATA LPXX; SET LEGEND.LPXX; RUN;
DATA LPXX; SET LEGEND.LPXX; RUN;

PROC EXPORT
		DATA=LPXX
		OUTFILE='T:\SAS\Deferment and Forbearance Groups.xlsx'
		REPLACE;
RUN;

PROC EXPORT
		DATA=LPXX
		OUTFILE='T:\SAS\Deferment and Forbearance Groups.xlsx'
		REPLACE;
RUN;
