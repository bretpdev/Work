LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE ADXX AS
		SELECT 
			*
		FROM
			PKUB.ADXX_PCV_ATY_ADJ ADXX
		WHERE
			BF_SSN IN ('XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX')
			AND LN_SEQ = X
		ORDER BY
			BF_SSN,
			LD_FAT_ADJ_REQ_BEG
	;

	CREATE TABLE NHXXXX AS
		SELECT 
			BF_SSN,
			LD_FAT_ADJ_REQ_BEG,
			LF_USR_FAT_ADJ_REQ
		FROM
			ADXX
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA = LEGEND.NHXXXX
		OUTFILE = 'T:\SAS\NH XXXX.XLSX'
		REPLACE;
	SHEET = 'SELECT_ATTRIBUTES';
RUN;

PROC EXPORT
		DATA = LEGEND.ADXX
		OUTFILE = 'T:\SAS\NH XXXX.XLSX'
		REPLACE;
	SHEET = 'DUMP';
RUN;
