			PKUB.RS10_BR_RPD
		WHERE
			LC_STA_RPST10 = 'A'
			AND BF_SSN IN 
				(
					'138524171',
					'252510017',
					'286880496',
					'501137845',
					'519027863',
					'519112125',
					'528252485',
					'528333848',
					'528438213',
					'528554801',
					'528650272',
					'528686862',
					'528919515',
					'529572339',
					'567856742',
					'609408528',
					'613092302'
				)
	;
