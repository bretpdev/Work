LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE POLL AS
		SELECT DISTINCT
			LNXX.BF_SSN
		FROM
			PKUB.LNXX_LON LNXX
			JOIN PKUB.DWXX_DW_CLC_CLU DWXX
				ON LNXX.BF_SSN = DWXX.BF_SSN
				AND LNXX.LN_SEQ = DWXX.LN_SEQ
			JOIN PKUB.LNXX_LON_RPS LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN
				AND LNXX.LN_SEQ = LNXX.LN_SEQ
		WHERE
			LNXX.LA_CUR_PRI + DWXX.WA_TOT_BRI_OTS > X
			AND LNXX.LC_STA_LONXX = 'R'
			AND LNXX.LC_TYP_SCH_DIS IN ('IB', 'IL', 'CX', 'CX', 'CX', 'CQ', 'CA', 'CP', 'IX', 'IP') 
			AND LNXX.LC_STA_LONXX = 'A'
			AND MONTH(LNXX.LD_LON_IBR_ENT) = X
	;
QUIT;
ENDRSUBMIT;

DATA POLL; SET LEGEND.POLL; RUN;

PROC EXPORT
		DATA=POLL
		OUTFILE='T:\POLL.XLSX'
		REPLACE;
RUN;

