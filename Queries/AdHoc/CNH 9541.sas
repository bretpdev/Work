LIBNAME XL 'T:\NH XXXX.xlsx';

DATA WORK.XLSOURCE (DROP=CLUID);
      SET XL.'A$'N;
RUN;

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.XLSOURCE; *Send data to legend;
	SET XLSOURCE;
RUN;

RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
LIBNAME PKUS DBX DATABASE=DNFPUTDL OWNER=PKUS;
PROC SQL;
	CREATE TABLE DUMP AS
		SELECT 
			LNXX.*
		FROM
			XLSOURCE XL
			INNER JOIN PKUB.LNXX_LON LNXX
				ON XL.BF_SSN = LNXX.BF_SSN
				AND XL.LN_SEQ = LNXX.LN_SEQ
		;
	;
QUIT;
ENDRSUBMIT;

DATA LNXX_DUMP; 
	SET LEGEND.DUMP; 
RUN;

PROC EXPORT
		DATA=LNXX_DUMP
		OUTFILE='T:\SAS\CNH_XXXX_LNXXdump.xlsx'
		REPLACE;
RUN;	
