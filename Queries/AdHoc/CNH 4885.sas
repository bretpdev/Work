LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT
			WF_QUE,
			WF_SUB_QUE,
			WN_CTL_TSK,
			PF_REQ_ACT
		FROM
			PKUB.WQXX_TSK_QUE
		WHERE
			WF_QUE IN ('JN','XR')
			AND WF_SUB_QUE = 'XX'
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.NHXXXX
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;
