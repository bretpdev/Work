PROC IMPORT OUT = WORK.SOURCE
            DATAFILE = "T:NH 23374.xlsx" 
            DBMS = xlsx REPLACE;
   			SHEET = 'Raw Data'; 
RUN;

LIBNAME  DUSTER  REMOTE  SERVER=DUSTER SLIBREF=WORK;
DATA DUSTER.SOURCE;
SET SOURCE;
RUN;

LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
CREATE TABLE DEMO AS
	SELECT DISTINCT
		LN40.BF_SSN				AS SSN,
		LN40.LN_SEQ				AS Loan_Sequence,
		LN40.LN_SEQ_CLM_PCL AS CLAIM_NUMBER
	FROM	
		OLWHRM1.LN40_LON_CLM_PCL LN40
		INNER JOIN SOURCE S
			ON S.SSN = LN40.BF_SSN
			AND S.LOAN_SEQUENCE = LN40.LN_SEQ
	WHERE
		LN40.LD_SBM_CLM_PCL BETWEEN INPUT('06/01/2015', DATE9.) AND INPUT('06/30/2015', DATE9.)
		AND (LN40.LC_TYP_REC_CLP_LON = 1 OR LN40.LC_TYP_REC_CLP_LON  = 6)
		AND LN10.IF_GTR IN  ('000706','000708','000730','000731','000751','000755','000800','000927','000951')
						

	;
QUIT;
ENDRSUBMIT;

DATA DEMO;
SET DUSTER.DEMO;
RUN;
