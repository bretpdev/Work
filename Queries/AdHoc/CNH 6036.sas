LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
DATA PDXX;
	SET PKUB.PDXX_PRS_ADR_EML;
	WHERE 
		DX_ADR_EML_UC LIKE '%@JBHUT.COM'
		OR DX_ADR_EML_UC LIKE 'JBHBLOCKEMAIL.COM@JBHUT.COM'
		OR DX_ADR_EML_UC LIKE '%BLOCKEMAIL%'	
		OR DX_ADR_EML_UC LIKE 'JBH%'
	;
RUN;

DATA PDXX;
	SET PKUB.PDXX_PRS_PND_DMO PDXX;
	WHERE 
		UPCASE(DX_ADR_EML_PND) LIKE '%@JBHUT.COM'
		OR UPCASE(DX_ADR_EML_PND) LIKE 'JBHBLOCKEMAIL.COM@JBHUT.COM'
		OR UPCASE(DX_ADR_EML_PND) LIKE '%BLOCKEMAIL%'	
		OR UPCASE(DX_ADR_EML_PND) LIKE 'JBH%'
	;
RUN;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.BADEMAIL
		OUTFILE='T:\Bad Email Address.XLSX'
		REPLACE;
RUN;
