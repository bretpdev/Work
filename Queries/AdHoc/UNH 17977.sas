LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE ISD AS
		SELECT
			PD10.DF_SPE_ACC_ID
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN16_LON_DLQ_HST LN16
				ON PD10.DF_PRS_ID = LN16.BF_SSN
				AND LN16.LC_STA_LON16 = '1'
				AND LN16.LN_DLQ_MAX > 15
			JOIN OLWHRM1.DF10_BR_DFR_REQ DF10
				ON PD10.DF_PRS_ID = DF10.BF_SSN
				AND DF10.LC_DFR_STA = 'A'
				AND DF10.LC_STA_DFR10 = 'A'
				AND DF10.LC_DFR_TYP IN ('15','16','18','19','22','23')
			JOIN OLWHRM1.LN50_BR_DFR_APV LN50
				ON LN16.BF_SSN = LN50.BF_SSN
				AND LN16.LN_SEQ = LN50.LN_SEQ
				AND DF10.LF_DFR_CTL_NUM = LN50.LF_DFR_CTL_NUM
				AND LN50.LC_STA_LON50 = 'A'
				AND LN50.LD_DFR_END BETWEEN '01APR2013'D AND '30JUN2013'D
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.ISD
		OUTFILE='T:\SAS\NHUH 17977.XLSX'
		REPLACE;
RUN;
