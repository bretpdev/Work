LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;


PROC SQL;
	
	CREATE TABLE LN65DUMP AS
		SELECT LN65.*
		FROM
			OLWHRM1.LN65_LON_RPS LN65
			INNER JOIN OLWHRM1.PD10_PRS_NME PD10
			ON PD10.DF_PRS_ID = LN65.BF_SSN
		WHERE 
		PD10.DF_SPE_ACC_ID = '5116766968'
	;

	CREATE TABLE RS05DUMP AS
		SELECT RS05.*
		FROM
			OLWHRM1.RS05_IBR_RPS RS05
			INNER JOIN OLWHRM1.PD10_PRS_NME PD10
			ON PD10.DF_PRS_ID = RS05.BF_SSN
		WHERE 
		PD10.DF_SPE_ACC_ID = '5116766968'
	;

	CREATE TABLE RS10DUMP AS
		SELECT RS10.*
		FROM
			OLWHRM1.RS10_BR_RPD RS10
			INNER JOIN OLWHRM1.PD10_PRS_NME PD10
			ON PD10.DF_PRS_ID = RS10.BF_SSN
		WHERE 
		PD10.DF_SPE_ACC_ID = '5116766968'
	;

	CREATE TABLE RS20DUMP AS
		SELECT RS20.*
		FROM
			OLWHRM1.RS20_IBR_IRL_LON RS20
			INNER JOIN OLWHRM1.PD10_PRS_NME PD10
			ON PD10.DF_PRS_ID = RS20.BF_SSN
		WHERE 
		PD10.DF_SPE_ACC_ID = '5116766968'
	;
QUIT;

ENDRSUBMIT;

DATA LN65DUMP; SET DUSTER.LN65DUMP; RUN;

PROC EXPORT	
		DATA=DUSTER.LN65DUMP
		OUTFILE='T:\SAS\NH 22848 data dump.xlsx'
		REPLACE;
RUN;

DATA RS05DUMP; SET DUSTER.RS05DUMP; RUN;

PROC EXPORT	
		DATA=DUSTER.RS05DUMP
		OUTFILE='T:\SAS\NH 22848 data dump.xlsx'
		REPLACE;
RUN;

DATA RS10DUMP; SET DUSTER.RS10DUMP; RUN;

PROC EXPORT	
		DATA=DUSTER.RS10DUMP
		OUTFILE='T:\SAS\NH 22848 data dump.xlsx'
		REPLACE;
RUN;

DATA RS20DUMP; SET DUSTER.RS20DUMP; RUN;

PROC EXPORT	
		DATA=DUSTER.RS20DUMP
		OUTFILE='T:\SAS\NH 22848 data dump.xlsx'
		REPLACE;
RUN;
