LIBNAME XL 'T:\KU type XX adj needed.xlsx';
DATA XLSOURCE;
	SET XL.'KU type XX adj needed$'N;
RUN;
LIBNAME XL CLEAR;

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.XLSOURCE;
	SET XLSOURCE;
RUN;

RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE POP AS
		SELECT DISTINCT
			XLS.*
			,LNXX.LD_LON_X_DSB
		FROM
			XLSOURCE XLS
			LEFT JOIN PKUB.LNXX_LON LNXX
				ON XLS.BF_SSN = LNXX.BF_SSN
				AND XLS.LN_SEQ = LNXX.LN_SEQ
		;
QUIT;
ENDRSUBMIT;

PROC EXPORT
	DATA=LEGEND.POP
	OUTFILE='T:\SAS\CNH XXXXX.xlsx'
	LABEL 
	DBMS=EXCEL
	REPLACE;
	SHEET="CNH XXXXX";
RUN;
