LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE SCRA AS
		SELECT
			LNXX.BF_SSN,
			LNXX.LN_SEQ
		FROM
			PKUB.LNXX_INT_RTE_HST LNXX
		WHERE
			LNXX.LC_STA_LONXX = 'A'
			AND LNXX.LC_INT_RDC_PGM = 'M' 
			AND LNXX.LD_STA_LONXX >= 'XXMARXXXX'D
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.SCRA
		OUTFILE='T:\SAS\SCRA Rate Applied on or After X-X-XXXX.xlsx'
		REPLACE;
RUN;
