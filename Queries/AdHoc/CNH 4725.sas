LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE LNXX AS
		SELECT
			BF_SSN,
			LN_SEQ,
			'' AS LF_FED_CLC_RSK,
			DATETIME() AS LF_LST_DTS_LNXX FORMAT DATETIMEXX.X
		FROM
			PKUB.LNXX_LON LNXX
		WHERE
			BF_SSN IN ('XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX')
	;

	CREATE TABLE FRXX AS
		SELECT
			BF_SSN,
			LN_SEQ,
			X AS LN_VIT_CHG_HST_SEQ,
			LNXX.LD_LON_X_DSB AS LD_LON_X_DSB_OLD,
			'' AS LD_LON_X_DSB_NEW,
			LNXX.LC_ACA_GDE_LEV AS LC_ACA_GDE_LEV_OLD,
			'' AS LC_ACA_GDE_LEV_NEW,
			'' AS LF_FFY_X_DSB_OLD,
			'' AS LF_FFY_X_DSB_NEW,
			LNXX.LF_FED_CLC_RSK AS LF_FED_CLC_RSK_OLD,
			'' AS LF_FED_CLC_RSK_NEW,
			'' AS LI_VIT_CHG_PRT,
			TODAY() AS LD_VIT_CHG_PRT_REQ FORMAT MMDDYYXX.,
			'A' AS LC_STA_FRXX,
			'' AS LF_LST_USR_FRXX,
			DATETIME() AS LF_LST_DTS_FRXX FORMAT DATETIMEXX.X
		FROM
			PKUB.LNXX_LON LNXX
		WHERE
			BF_SSN IN ('XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX')
	;

	CREATE TABLE FRXX_DUMP AS
		SELECT
			*
		FROM
			PKUB.FRXX_VIT_CHG_HST FRXX
		WHERE
			BF_SSN IN ('XXXXXXXXX','XXXXXXXXX','XXXXXXXXX','XXXXXXXXX')
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.LNXX
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;

PROC EXPORT
		DATA=LEGEND.FRXX
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;

PROC EXPORT
		DATA=LEGEND.FRXX_DUMP
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;

			
