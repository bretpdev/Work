LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE MISSING AS
		SELECT DISTINCT
			PD10.DF_PRS_ID
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			LEFT JOIN OLWHRM1.PD30_PRS_ADR PD30
				ON PD10.DF_PRS_ID = PD30.DF_PRS_ID
			LEFT JOIN OLWHRM1.LN10_LON LN10
				ON PD10.DF_PRS_ID = LN10.BF_SSN
		WHERE
			PD30.DF_PRS_ID IS NULL
			AND LN10.BF_SSN IS NULL
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.MISSING
		OUTFILE='T:\SAS\NO PD30.xlsx'
		REPLACE;
RUN;
			
