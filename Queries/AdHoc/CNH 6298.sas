LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE LON_ADD AS
		SELECT DISTINCT
			LNXX.BF_SSN,
			LNXX.LN_SEQ
		FROM
			PKUB.LNXX_LON LNXX
			JOIN PKUB.RSXX_BR_RPD RSXX
				ON LNXX.BF_SSN = RSXX.BF_SSN
		WHERE
			LNXX.LD_LON_ACL_ADD > 'XXDECXXXX'D
			AND RSXX.LC_STA_RPSTXX = 'A'
	;
QUIT;
ENDRSUBMIT;

DATA LON_ADD; SET LEGEND.LON_ADD; RUN;

PROC EXPORT
		DATA=LON_ADD
		OUTFILE='T:\Loans added after XXDECXXXX.XLSX'
		REPLACE;
RUN;
