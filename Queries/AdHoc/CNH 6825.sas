LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;


PROC SQL;
	
	CREATE TABLE DLFDL AS
		SELECT DISTINCT
			LNXX.BF_SSN,
			LNXX.LN_SEQ,
			LNXX.LA_CUR_PRI,
			AYXX.PF_REQ_ACT,
			AYXX.PF_RSP_ACT
		FROM
			PKUB.LNXX_LON LNXX
			INNER JOIN PKUB.AYXX_BR_LON_ATY AYXX 
				ON AYXX.BF_SSN = LNXX.BF_SSN
		WHERE 
		LNXX.LA_CUR_PRI > X 
		AND AYXX.PF_REQ_ACT = 'DLFDL'
		AND AYXX.PF_RSP_ACT = 'PRNTD'
	;
QUIT;

ENDRSUBMIT;

DATA DLFDL; SET LEGEND.DLFDL; RUN;

PROC EXPORT	
		DATA=LEGEND.DLFDL
		OUTFILE='T:\SAS\SSAE XX Final Demand Borrower.xlsx'
		REPLACE;
RUN;
