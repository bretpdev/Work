LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE CLMS AS
		SELECT
			LN40.BF_SSN,
			LN40.LN_SEQ,
			LN10.IF_GTR,
			CL10.LF_USR_ASN_CLM_PCL,
			LN40.LD_CLM_ORG_SBM,
			LN40.LN_SEQ_CLM_PCL,
			LN40.LD_SBM_CLM_PCL,
			LN40.LD_CRT_CLM_PCL
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN40_LON_CLM_PCL LN40
				ON PD10.DF_PRS_ID = LN40.BF_SSN
			LEFT JOIN OLWHRM1.CL10_CLM_PCL CL10
				ON LN40.BF_SSN = CL10.BF_SSN
				AND LN40.LN_SEQ_CLM_PCL = CL10.LN_SEQ_CLM_PCL
			LEFT JOIN OLWHRM1.LN10_LON LN10
				ON LN10.BF_SSN = LN40.BF_SSN
				AND LN10.LN_SEQ = LN40.LN_SEQ
		WHERE
/*			LN40.LD_CLM_ORG_SBM BETWEEN '01DEC2014'D AND '28FEB2015'D*/
			(LN40.LD_SBM_CLM_PCL BETWEEN '01DEC2014'D AND '28FEB2015'D
			OR LN40.LD_CRT_CLM_PCL BETWEEN '01DEC2014'D AND '28FEB2015'D)
/*			AND PD10.DF_SPE_ACC_ID = '4588664935'*/
	;
QUIT;
ENDRSUBMIT;

DATA CLMS; SET DUSTER.CLMS; RUN;

PROC EXPORT
		DATA=CLMS
		OUTFILE='T:\Claims Submitted.XLSX'
		REPLACE;
RUN;
