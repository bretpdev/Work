%LET RPTLIB = T:\SAS;
LIBNAME  LEGEND  REMOTE  SERVER=LEGEND  SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE DEMO AS
		SELECT DISTINCT	
			LNXX.BF_SSN
			,LNXX.LN_SEQ
		FROM	
			PKUB.LNXX_LON LNXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
				ON LNXX.BF_SSN = PDXX.DF_PRS_ID
			INNER JOIN PKUB.DWXX_DW_CLC_CLU DWXX
				ON LNXX.BF_SSN = DWXX.BF_SSN
				AND LNXX.LN_SEQ = DWXX.LN_SEQ
		WHERE	
			LNXX.LC_STA_LONXX = 'R'
			AND LNXX.LA_CUR_PRI > X
			AND	DWXX.WC_DW_LON_STA IN ('XX','XX','XX','XX','XX','XX','XX','XX','XX','XX')
			AND LNXX.IC_LON_PGM NOT IN 
				(
				'DLUCNS','DLSCNS','CNSLDN','DSCON','DLUSPL','DUCON','SPCNSL', 
				'SUBCNS','SUBSPC','UNCNS','DLSPCN','DLSSPL','DLCNSL','DLPCNS', 
				'UNSPC','DLSCST','DLSCUN','DLSCPL','DLSCPG','DLSCSL','DLSCSC', 
				'DLSCUC', 'DLSCCN'
				)
	;
ENDRSUBMIT;

PROC EXPORT 
	DATA= LEGEND.DEMO
	OUTFILE= "&RPTLIB\NH XXXX.xlsx" 
	DBMS=EXCEL 
	REPLACE;
	SHEET="Interest_Accruing";
RUN;
