%LET RPTLIB = T:\SAS;
LIBNAME XL 'T:\BANA ACH DCR Borrowers.xlsx';
DATA WORK.XLSOURCE;
	SET XL.'Sheet1$'N;
	IF BF_SSN ^= '';
RUN;
LIBNAME XL CLEAR;
LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK; /*live*/
DATA DUSTER.XLSOURCE; *Send data to Duster;
	SET XLSOURCE;
RUN;
RSUBMIT;
%LET DB = DLGSUTWH; /*live*/
LIBNAME OLWHRM1 DB2 DATABASE=&DB OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LN72_dump AS
	SELECT
		LN72.*
	FROM
		WORK.XLSOURCE XLS
		INNER JOIN OLWHRM1.LN72_INT_RTE_HST LN72
			ON XLS.BF_SSN = LN72.BF_SSN
	;
QUIT;
ENDRSUBMIT;
PROC EXPORT
	DATA=Duster.LN72_dump
	OUTFILE="&RPTLIB\UNH 53358.xlsx"
	DBMS = EXCEL
	REPLACE;
RUN;
