LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE EOM AS
		SELECT
			BF_SSN,
			LN_SEQ,
			LD_FAT_EFF,
			LA_FAT_CUR_PRI
		FROM
			OLWHRM1.LN90_FIN_ATY
		WHERE	
			LD_FAT_EFF GE '01AUG2014'D
			AND LD_FAT_EFF LE '31AUG2014'D
			AND PC_FAT_TYP = '18' 
			AND PC_FAT_SUB_TYP = '01' 
			AND LC_STA_LON90 = 'A'
			AND LC_FAT_REV_REA = ''
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.EOM
		OUTFILE='T:\SAS\Rebate Data for EOM August 2014.XLSX'
		REPLACE;
RUN;
