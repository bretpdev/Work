LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			LNXX.PC_FAT_SUB_TYP,
			LNXX.LD_FAT_APL,
			LNXX.LA_FAT_NSI + LNXX.LA_FAT_CUR_PRI AS AMT_APPLIED
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.LNXX_FIN_ATY LNXX
				ON PDXX.DF_PRS_ID = LNXX.BF_SSN
		WHERE
			LNXX.PC_FAT_TYP = 'XX'
			AND LNXX.PC_FAT_SUB_TYP IN ('XX','XX')
			AND LNXX.LD_FAT_APL BETWEEN 'XXJANXXXX'D AND 'XXDECXXXX'D
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.NHXXXX
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;
