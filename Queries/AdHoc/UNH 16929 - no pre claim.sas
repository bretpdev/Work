RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE NODELQ AS
		SELECT
			NOPR.BF_SSN,
			NOPR.LN_SEQ,
			NOPR.AC_LON_STA_TYP,
			NOPR.LN_SEQ_CLM_PCL,
			NOPR.PRECLAIM_DATE,
			COALESCE(LN16.LN_DLQ_MAX,0) AS LN_DLQ_MAX,
			LN10.LA_CUR_PRI,
			LN10.LC_STA_LON10
		FROM
			NOPRECLAIM NOPR
			JOIN OLWHRM1.LN10_LON LN10
				ON NOPR.BF_SSN = LN10.BF_SSN
				AND NOPR.LN_SEQ = LN10.LN_SEQ
			LEFT JOIN OLWHRM1.LN16_LON_DLQ_HST LN16 
				ON LN10.BF_SSN = LN16.BF_SSN
				AND LN10.LN_SEQ = LN16.LN_SEQ
				AND LN16.LC_STA_LON16 = '1'
	;
QUIT;

ENDRSUBMIT;
			
DATA NODELQ; SET DUSTER.NODELQ; RUN;

PROC EXPORT
		DATA = NODELQ
		OUTFILE = 'T:\SAS\NH 16929 NOT DELQ.XLSX'
		REPLACE;
RUN;
