PROC SQL;
	CREATE TABLE RS05 AS
		SELECT DISTINCT
			RS05.BF_SSN			AS RS05_SSN
			,RS05.BD_CRT_RS05	AS RS05_DATE
			,RS10.BF_SSN 		AS RS10_SSN
			,RS10.BD_CRT_RS05 	AS RS10_DATE
		FROM 
			OLWHRM1.RS05_IBR_RPS RS05
			INNER JOIN OLWHRM1.RS10_BR_RPD RS10
				ON RS05.BF_SSN = RS10.BF_SSN
		WHERE
			RS10.BD_CRT_RS05 IS NULL
			AND RS10.LC_STA_RPST10 = 'A'
			AND RS05.BC_STA_RS05 = 'A'
	;
