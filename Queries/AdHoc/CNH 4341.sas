LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE FSXX AS
		SELECT   
			FSX.BF_SSN, 
			FSX.LN_SEQ
		FROM
			PKUB.FSXX_DL_LON FSX,
			PKUB.LNXX_LON LNX
		WHERE    
			FSX.BF_SSN=LNX.BF_SSN
			AND FSX.LN_SEQ=LNX.LN_SEQ
			AND LNX.IC_LON_PGM IN ('DLSPCN','DLSSPL','DLUSPL','DLPCNS','DLSCNS','DLUCNS','DLCNSL')
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.FSXX
		OUTFILE='T:\SAS\CR XXXX DCR.xlsx'
		REPLACE;
RUN;
