LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LN65 AS
		SELECT
			*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN65_LON_RPS LN65
				ON PD10.DF_PRS_ID = LN65.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('5659586046','4901908954','5693570294','5258135999','5509165202','4498560380')
	;

	CREATE TABLE LN66 AS
		SELECT
			*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN66_LON_RPS_SPF LN66
				ON PD10.DF_PRS_ID = LN66.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('5659586046','4901908954','5693570294','5258135999','5509165202','4498560380')
	;

	CREATE TABLE RS05 AS
		SELECT
			*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.RS05_IBR_RPS RS05
				ON PD10.DF_PRS_ID = RS05.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('5659586046','4901908954','5693570294','5258135999','5509165202','4498560380')
	;

	CREATE TABLE RS10 AS
		SELECT
			*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.RS10_BR_RPD RS10
				ON PD10.DF_PRS_ID = RS10.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('5659586046','4901908954','5693570294','5258135999','5509165202','4498560380')
	;

	CREATE TABLE RS20 AS
		SELECT
			*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.RS20_IBR_IRL_LON RS20
				ON PD10.DF_PRS_ID = RS20.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('5659586046','4901908954','5693570294','5258135999','5509165202','4498560380')
	;

QUIT;
ENDRSUBMIT;

DATA LN65; SET DUSTER.LN65; RUN;
DATA LN66; SET DUSTER.LN66; RUN;
DATA RS05; SET DUSTER.RS05; RUN;
DATA RS10; SET DUSTER.RS10; RUN;
DATA RS20; SET DUSTER.RS20; RUN;

PROC EXPORT
		DATA=LN65
		OUTFILE='T:\Align IBR DCR Data Dump.XLSX'
		REPLACE;
RUN;

