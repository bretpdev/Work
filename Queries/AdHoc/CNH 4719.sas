LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE DEAD AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			PDXX.DM_PRS_LST,
			TODAY() - PDXX.DD_DTH_STA AS STA_DAYS
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.PDXX_GTR_DTH PDXX
				ON PDXX.DF_PRS_ID = PDXX.DF_PRS_ID
		WHERE
			PDXX.DC_DTH_STA = 'XX'
	;
QUIT;

ENDRSUBMIT;

DATA DEAD; SET LEGEND.DEAD; RUN;

PROC EXPORT
		DATA=DEAD
		OUTFILE='T:\NH XXXX OUTPUT.XLSX'
		REPLACE;
RUN;
