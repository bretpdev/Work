LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE REDEF AS
		SELECT
			LNXX.BF_SSN,
			LNXX.LN_SEQ
		FROM
			PKUB.LNXX_LON LNXX
			JOIN PKUB.LNXX_LON_DLQ_HST LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN
				AND LNXX.LN_SEQ = LNXX.LN_SEQ
			LEFT JOIN PKUB.LNXX_FIN_ATY LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN
				AND LNXX.LN_SEQ = LNXX.LN_SEQ
				AND LNXX.PC_FAT_TYP = 'XX'
				AND LNXX.PC_FAT_SUB_TYP = 'XX'
		WHERE
			LNXX.LC_STA_LONXX = 'D'
			AND LNXX.LN_DLQ_MAX > XXX
			AND LNXX.BF_SSN IS NULL

	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.REDEF
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;
