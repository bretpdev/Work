PROC IMPORT OUT= WORK.source
            DATAFILE= "T:\UPDATE Billing Batch Critical XX XXXX.xlsx" 
            DBMS=EXCEL REPLACE;
     RANGE="SheetX$"; 
     GETNAMES=YES;
     MIXED=NO;
     SCANTEXT=YES;
     USEDATE=YES;
     SCANTIME=YES;
RUN;

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.SOURCE; *Send data to Duster;
SET SOURCE;
RUN;

RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE LNXX AS
		SELECT	
			LNXX.BF_SSN,
			LNXX.LN_SEQ,
			LNXX.LD_BIL_CRT,
			LNXX.LN_SEQ_BIL_WI_DTE,
			LNXX.LC_STA_LONXX AS OLD_LC_STA_LONXX,
			'I' AS NEW_LC_STA_LONXX
		FROM
			 SOURCE S
		INNER JOIN PKUB.LNXX_LON_BIL_CRF LNXX
			ON LNXX.BF_SSN = S.BF_SSN
			AND LNXX.LD_BIL_CRT = S.LD_BIL_CRT
			AND LNXX.LN_SEQ_BIL_WI_DTE = S.LN_SEQ_BIL_WI_DTE
;
QUIT;
ENDRSUBMIT;

DATA LNXX;
SET LEGEND.LNXX;
RUN;

PROC EXPORT DATA = WORK.LNXX 
            OUTFILE = "T:\LNXX_XX-XX-XXXX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="SheetX"; 
RUN;
