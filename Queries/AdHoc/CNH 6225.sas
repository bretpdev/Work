LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE TENXX AS
		SELECT
			*
		FROM
			PKUB.LNXX_FIN_ATY LNXX
		WHERE
			LD_FAT_EFF >= 'XXJANXXXX'D
		HAVING
			CATX('/',PC_FAT_TYP,PC_FAT_SUB_TYP) IN 
				(
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX',
					'XX/XX'
				)
	;
QUIT;
ENDRSUBMIT;

DATA TENXX; SET LEGEND.TENXX; RUN;

PROC EXPORT
		DATA=TENXX
		OUTFILE='T:\XXXX transactions.XLSX'
		REPLACE;
RUN;
