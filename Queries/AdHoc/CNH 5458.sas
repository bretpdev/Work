/*NOTE:  you must open the spreadsheet using password CXaXXXX before running this job*/

LIBNAME XL 'T:\Copy of Cornerstone.xlsx';
LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;

DATA LEGEND.XL; SET XL.'SheetX$'N; RUN;

RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE DMCS AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			LNXX.BF_SSN,
			LNXX.LN_SEQ,
			LNXX.LA_CUR_PRI + DWXX.WA_TOT_BRI_OTS AS BALANCE
		FROM
			XL
			JOIN PKUB.PDXX_PRS_NME PDXX
				ON XL.SSN = PDXX.DF_PRS_ID
			JOIN PKUB.LNXX_LON LNXX
				ON XL.SSN = LNXX.BF_SSN
			JOIN PKUB.DWXX_DW_CLC_CLU DWXX
				ON LNXX.BF_SSN = DWXX.BF_SSN
				AND LNXX.LN_SEQ = DWXX.LN_SEQ
	;
QUIT;

ENDRSUBMIT;

DATA DMCS; SET LEGEND.DMCS; RUN;

PROC EXPORT
		DATA=DMCS
		OUTFILE='T:\SAS\Princ + Interest for DMCS Redefaults.XLSX'
		REPLACE;
RUN;
