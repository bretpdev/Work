PROC IMPORT OUT= WORK.ORIG_DCR
            DATAFILE = "T:\Original DCR.xlsx"
            DBMS=EXCEL REPLACE;
        RANGE="RS10$";
        GETNAMES=YES;
        MIXED=NO;
        SCANTEXT=YES;
        USEDATE=YES;
        SCANTIME=YES;
RUN;

LIBNAME  DUSTER  REMOTE  SERVER=DUSTER SLIBREF=WORK;
DATA DUSTER.ORIG_DCR; *Send data to Duster;
SET ORIG_DCR;
RUN;

RSUBMIT DUSTER;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
    CREATE TABLE RS10 AS
        SELECT  
            RS10.BF_SSN,
			RS10.LN_RPS_SEQ,
			RS10.BD_CRT_RS05 as BD_CRT_RS05_CURRENT,
			'03/31/2016' as BD_CRT_RS05_NEW

        FROM  
			ORIG_DCR DCR  
			INNER JOIN  OLWHRM1.RS10_BR_RPD RS10
				ON RS10.BF_SSN = DCR.BF_SSN
				AND RS10.LN_RPS_SEQ = DCR.LN_RPS_SEQ
			INNER JOIN OLWHRM1.LN65_LON_RPS LN65
				ON RS10.BF_SSN = LN65.BF_SSN
				AND RS10.LN_RPS_SEQ = LN65.LN_RPS_SEQ
				AND LN65.LC_TYP_SCH_DIS = 'IB'
		WHERE
			RS10.LC_STA_RPST10 = 'A'
			AND RS10.BD_CRT_RS05 = DCR.BD_CRT_RS05_Updated
			
    ;
QUIT;

ENDRSUBMIT;

DATA RS10;
SET DUSTER.RS10;
RUN;

PROC EXPORT DATA= WORK.RS10 
            OUTFILE= "T:\RS10_BR_RPD_DCR.xlsx" 
            DBMS=EXCEL REPLACE;
     SHEET="RS10"; 
RUN;
