PROC IMPORT OUT = WORK.SOURCE
            DATAFILE = "T:\NH XXXX Borrowers.xlsx" ;
   			SHEET = 'SheetX$'n; 
RUN;

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.SOURCE; *Send data to Legend;
SET SOURCE;
RUN;


RSUBMIT LEGEND;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT	
			LNXX.*
		FROM
			 SOURCE S
		INNER JOIN PKUB.LNXX_LON LNXX
			ON LNXX.BF_SSN = S.SSN
;
QUIT;
ENDRSUBMIT;

PROC EXPORT DATA = LEGEND.NHXXXX 
            OUTFILE = "T:\SAS\NH XXXX.xls" 
            DBMS = EXCEL
			REPLACE;
     SHEET="A"; 
RUN;
