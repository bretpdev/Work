LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE IDR AS
		SELECT DISTINCT
			PDXX.DF_PRS_ID AS SSN,
			PDXX.DM_PRS_X AS FIRST_NAME,
			PDXX.DM_PRS_LST AS LAST_NAME
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.AYXX_BR_LON_ATY ED
				ON PDXX.DF_PRS_ID = ED.BF_SSN
				AND ED.PF_REQ_ACT IN ('EDXXX','EDXXX','EDXXX','LRGED','GRCED')
			JOIN PKUB.AYXX_BR_LON_ATY PX
				ON ED.BF_SSN = PX.BF_SSN
				AND PX.PF_REQ_ACT = 'PXXXC'
				AND PX.LD_ATY_RSP > ED.LD_ATY_RSP
	;
QUIT;

ENDRSUBMIT;

DATA IDR; SET LEGEND.IDR; RUN;

PROC EXPORT
		DATA=IDR
		OUTFILE='T:\IDR.XLSX'
		REPLACE;
RUN;

		
