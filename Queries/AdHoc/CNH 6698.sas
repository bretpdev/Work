LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE RSXX AS
		SELECT
			PDXX.DF_SPE_ACC_ID,
			RSXX.*
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.RSXX_IBR_RPS RSXX
				ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE
			PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE RSXX AS
		SELECT
			PDXX.DF_SPE_ACC_ID,
			RSXX.*
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.RSXX_BR_RPD RSXX
				ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE
			PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE RSXX AS
		SELECT
			PDXX.DF_SPE_ACC_ID,
			RSXX.*
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.RSXX_IBR_IRL_LON RSXX
				ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE
			PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE LNXX AS
		SELECT
			PDXX.DF_SPE_ACC_ID,
			LNXX.*
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.LNXX_LON_RPS LNXX
				ON PDXX.DF_PRS_ID = LNXX.BF_SSN
		WHERE
			PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

QUIT;
ENDRSUBMIT;

DATA RSXX; SET LEGEND.RSXX; RUN;
DATA RSXX; SET LEGEND.RSXX; RUN;
DATA RSXX; SET LEGEND.RSXX; RUN;
DATA LNXX; SET LEGEND.LNXX; RUN;

PROC EXPORT
		DATA=RSXX
		OUTFILE='T:\IDR Tables.XLSX'
		REPLACE;
RUN;

