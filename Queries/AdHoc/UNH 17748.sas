
LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;

PROC SQL;
	CONNECT TO DB2 (DATABASE=DLGSUTWH);

	CREATE TABLE ONEL AS
		SELECT 
			*
		FROM 
			CONNECTION TO DB2 
				(
					SELECT
						*
					FROM	
						OLWHRM1.CT10_COL_MST

					FOR READ ONLY WITH UR
				)
	;

	CREATE TABLE COMP AS
		SELECT 
			*
		FROM 
			CONNECTION TO DB2 
				(
					SELECT
/*this list is easier on the eyes but they asked for a dump*/
/*						WQ10.WF_QUE,*/
/*						WQ15.WF_SUB_QUE,*/
/*						WQ10.WM_QUE_SHO,*/
/*						WQ15.WM_SUB_QUE_SHO,*/
/*						WQ10.WF_USR_OWN_QUE,*/
/*						WQ15.WF_SVR_SUB_QUE*/
						WQ10.*,
						WQ15.*
					FROM	
						OLWHRM1.WQ10_TSK_QUE_DFN WQ10
						JOIN OLWHRM1.WQ15_TSK_SUB_QUE WQ15
							ON WQ10.WF_QUE = WQ15.WF_QUE

					FOR READ ONLY WITH UR
				)
	;

	DISCONNECT FROM DB2;

QUIT;

ENDRSUBMIT;

DATA COMP; SET DUSTER.COMP; RUN;
DATA ONEL; SET DUSTER.ONEL; RUN;

PROC EXPORT
		OUTFILE = 'T:\SAS\NHUH 17748.XLS'
		DATA = ONEL
		REPLACE;
	SHEET = OneLINK;
QUIT;

PROC EXPORT
		OUTFILE = 'T:\SAS\NHUH 17748.XLS'
		DATA = COMP
		REPLACE;
	SHEET = Compass;
QUIT;
