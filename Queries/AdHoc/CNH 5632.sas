			INNER JOIN PKUB.AYXX_BR_LON_ATY AYXX 
				ON cf.LF_USR_REQ_ATY = AYXX.LF_USR_REQ_ATY 
				AND cf.BF_SSN = input(AYXX.BF_SSN, bestXX.)
				AND cf.PF_RSP_ACT = AYXX.PF_RSP_ACT
				AND cf.LF_PRF_BY = AYXX.LF_PRF_BY
	;
QUIT;
