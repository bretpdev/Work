LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT DISTINCT
			PDXX.DM_PRS_X,
			PDXX.DM_PRS_LST,
			PDXX.DF_SPE_ACC_ID,
			LNXX.LF_DOE_SCL_ORG,
			PDXX.DX_STR_ADR_X,
			PDXX.DX_STR_ADR_X,
			PDXX.DM_CT,
			PDXX.DC_DOM_ST,
			PDXX.DF_ZIP_CDE,
			PDXX.DM_FGN_CNY
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.PDXX_PRS_ADR PDXX
				ON PDXX.DF_PRS_ID = PDXX.DF_PRS_ID
			JOIN PKUB.LNXX_LON LNXX
				ON PDXX.DF_PRS_ID = LNXX.BF_SSN
		WHERE
			LNXX.LA_CUR_PRI > X
			AND LNXX.LC_STA_LONXX = 'R'
			AND LNXX.LF_DOE_SCL_ORG IN ('XXXXXXXX','XXXXXXXX','XXXXXXXX')
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.NHXXXX
		OUTFILE='T:\SAS\NH XXXX.XLSX'
		REPLACE;
RUN;
			
