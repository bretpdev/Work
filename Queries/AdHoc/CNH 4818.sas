LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE NHXXXX AS
		SELECT DISTINCT
			LNXX.BF_SSN,
			COALESCE(LNXX.LN_DLQ_MAX,X) AS LN_DLQ_MAX,
			LNXX.LC_STA_LONXX,
			DWXX.WC_DW_LON_STA,
			DWXX.WX_OVR_DW_LON_STA
		FROM
			PKUB.LNXX_LON LNXX
			JOIN PKUB.DWXX_DW_CLC_CLU DWXX
				ON LNXX.BF_SSN = DWXX.BF_SSN
				AND LNXX.LN_SEQ = DWXX.LN_SEQ
			LEFT JOIN PKUB.LNXX_LON_DLQ_HST LNXX
				ON DWXX.BF_SSN = LNXX.BF_SSN
				AND DWXX.LN_SEQ = LNXX.LN_SEQ
				AND LC_STA_LONXX = 'X'
				AND LD_DLQ_MAX = TODAY()-X
		WHERE
			LNXX.LC_STA_LONXX NOT IN ('P', 'J', 'I')
			AND LNXX.LA_CUR_PRI > X
			AND COALESCE(LNXX.LN_DLQ_MAX,X) >= TODAY() - 'XXJANXXXX'D + XXX
	;
QUIT;

ENDRSUBMIT;

DATA NHXXXX; SET LEGEND.NHXXXX; RUN;

PROC EXPORT
		DATA = NHXXXX
		OUTFILE = 'T:\NH XXXX.XLSX'
		REPLACE;
RUN;
