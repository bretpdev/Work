LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

DATA COMAKERS;
	SET PKUB.LNXX_EDS;
	WHERE LC_EDS_TYP = 'M';
RUN;

DATA COSIGNERS;
	SET PKUB.LNXX_EDS;
	WHERE LC_EDS_TYP = 'S';
RUN;

DATA LF_EDS;
	SET PKUB.LNXX_LON;
	WHERE LF_EDS <> '';
RUN;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.COMAKERS
		OUTFILE='T:\SAS\SAMPLING.XLSX'
		DBMS=EXCELXXXX
		REPLACE;
RUN;

PROC EXPORT
		DATA=LEGEND.COSIGNERS
		OUTFILE='T:\SAS\SAMPLING.XLSX'
		DBMS=EXCELXXXX
		REPLACE;
RUN;
