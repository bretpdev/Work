LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE NOPNT AS
		SELECT DISTINCT
			AY10.BF_SSN,
			GR10.IF_GTR,
			AY10.LN_ATY_SEQ,
			AY10.LC_STA_ACTY10,
			AY10.PF_REQ_ACT,
			AY10.PF_RSP_ACT,
			AY10.LF_LST_DTS_AY10 
		FROM
			OLWHRM1.AY10_BR_LON_ATY AY10
			JOIN OLWHRM1.GR10_RPT_LON_APL GR10
				ON AY10.BF_SSN = GR10.BF_SSN
		WHERE
			AY10.PF_REQ_ACT = 'NOPNT'
			AND GR10.IF_GTR IN ('000706','000708','000730','000731','000751','000755','000800','000951')
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.NOPNT
		OUTFILE='T:\NOPNT Query for Align.XLSX'
		REPLACE;
RUN;
