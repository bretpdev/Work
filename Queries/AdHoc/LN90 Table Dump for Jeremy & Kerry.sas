LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DB2 DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE LN90 AS
		SELECT
			LN90.*,
			LN10.IC_LON_PGM
		FROM
			PKUB.LN90_FIN_ATY LN90
			JOIN PKUB.LN10_LON LN10
				ON LN90.BF_SSN = LN10.BF_SSN
				AND LN90.LN_SEQ = LN10.LN_SEQ
		WHERE
			LN90.LD_FAT_APL BETWEEN '01SEP2016'D AND '30SEP2016'D
	;
QUIT;

ENDRSUBMIT;

DATA LN90;
	SET LEGEND.LN90;
RUN;

PROC EXPORT
		DATA=LN90
		OUTFILE='T:\SAS\LN90 EOM SEPTEMBER 2016.txt'
		DBMS=DLM;
		DELIMITER='|';
RUN;
