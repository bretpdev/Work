LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
LIBNAME XL 'T:\NEGAM DCR.xlsx';

DATA LEGEND.NEGAM; SET XL.'FSXX CHANGE$'N; RUN;

RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE FSXX AS
		SELECT
			FSXX.*
		FROM
			NEGAM NEG
			JOIN PKUB.FSXX_ICR_NEG_AMR FSXX
				ON NEG.BF_SSN = FSXX.BF_SSN
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.FSXX
		OUTFILE='T:\SAS\FSXX Data Dump.xlsx'
		REPLACE;
RUN;
