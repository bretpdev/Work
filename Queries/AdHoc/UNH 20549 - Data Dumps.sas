PROC SQL;
	CREATE TABLE LN54_DLQ AS
		SELECT
			PD10.DF_SPE_ACC_ID,
			LN54.*
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN54_LON_BBS LN54
				ON PD10.DF_PRS_ID = LN54.BF_SSN
		WHERE
			(PD10.DF_SPE_ACC_ID = '3631031422' AND LN54.LN_SEQ = 4)
			OR (PD10.DF_SPE_ACC_ID = '1792352078' AND LN54.LN_SEQ = 5)
			OR (PD10.DF_SPE_ACC_ID = '5026785528' AND LN54.LN_SEQ = 3)
			OR (PD10.DF_SPE_ACC_ID = '5214853091' AND LN54.LN_SEQ = 1)
	;

