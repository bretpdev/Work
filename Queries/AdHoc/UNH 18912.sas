LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE NH18912 AS
		SELECT
			DF_PRS_ID,
			BF_CRT_DTS_AY01
		FROM 
			OLWHRM1.AY01_BR_ATY AY01
		WHERE
			BC_ATY_TYP = 'AM'
			AND BD_ATY_PRF >= TODAY() - 7
			AND PF_ACT = 'KGNRL'
			AND BC_ATY_CNC_TYP = '10'
			AND BX_CMT LIKE('COULD NOT DECIPHER DEMOGRAPHIC INFORMATION.  {ACURINTR}%')
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.NH18912
		OUTFILE='T:\SAS\NH 18912.XLSX'
		REPLACE;
RUN;
