LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;

PROC SQL;
	CONNECT TO DB2 (DATABASE=DLGSUTWH);

	CREATE TABLE DEMO AS
		SELECT 
			*
		FROM 
			CONNECTION TO DB2 
				(
					SELECT DISTINCT
						PD10.DF_SPE_ACC_ID,
						PD10.DM_PRS_LST
						
					FROM	
						OLWHRM1.PD10_PRS_NME PD10
					INNER JOIN OLWHRM1.LN10_LON LN10
						ON LN10.BF_SSN = PD10.DF_PRS_ID
					INNER JOIN OLWHRM1.BR30_BR_EFT BR30
						ON LN10.BF_SSN = BR30.BF_SSN
						AND BR30.BC_EFT_STA = 'A'
					INNER JOIN OLWHRM1.LN83_EFT_TO_LON LN83
						ON LN83.BF_SSN = LN10.BF_SSN
						AND LN83.LN_SEQ = LN10.LN_SEQ
						AND LN83.BN_EFT_SEQ = BR30.BN_EFT_SEQ
					INNER JOIN OLWHRM1.PD32_PRS_ADR_EML PD32
						ON PD32.DF_PRS_ID = LN10.BF_SSN
						AND PD32.DI_VLD_ADR_EML = 'Y'
						AND PD32.DC_STA_PD32 = 'A'
					WHERE	
						LN10.LC_LON_SND_CHC ^= 'Y'
						AND LN10.LA_CUR_PRI > 0.00
						AND LN10.LC_STA_LON10 = 'R'
						AND LN83.LD_EFT_EFF_BEG >= '1/1/2014'
						

					FOR READ ONLY WITH UR
				)
	;

	DISCONNECT FROM DB2;

	/*%PUT  SQLXRC= >>> &SQLXRC <<< ||| SQLXMSG= >>> &SQLXMSG >>> ;  ** INCLUDES ERROR MESSAGES TO SAS LOG  ;*/
	/*%SQLCHECK;*/
QUIT;

ENDRSUBMIT;

PROC EXPORT DATA = DUSTER.DEMO 
            OUTFILE = "T:\SAS\NH 19397.xls" 
            DBMS = EXCEL
			REPLACE;
RUN;
