LIBNAME XL 'T:\Accounts need SSNsX.xlsx';

DATA SOURCE;
      SET XL.'SheetX$'N;
      Acct = STRIP(COMPRESS(DF_SPE_ACC_ID,'-'));
      FORMAT Acct $XX.;
      KEEP Acct DF_SPE_ACC_ID;
RUN;

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=WORK;
DATA LEGEND.SOURCE; *Send data to legend;
SET SOURCE;
RUN;


RSUBMIT;
LIBNAME pkub DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE POP AS
		SELECT	
			S.DF_SPE_ACC_ID,
			PDXX.DF_PRS_ID
		FROM
			 SOURCE S
		INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_SPE_ACC_ID = S.Acct
;
QUIT;
ENDRSUBMIT;

PROC EXPORT DATA = LEGEND.POP 
            OUTFILE = "T:\ACCOUNTS WITH SSNX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="SheetX"; 
RUN;
