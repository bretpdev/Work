LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE RX AS
		SELECT
			WQXX.WF_QUE,
			WQXX.WF_SUB_QUE,
			WQXX.WN_CTL_TSK,
			WQXX.PF_REQ_ACT
		FROM
			PKUB.WQXX_TSK_QUE WQXX
		WHERE
			WQXX.WF_QUE = 'RX'
			AND WQXX.WF_SUB_QUE = 'XX'
	;
QUIT;

ENDRSUBMIT;

DATA RX; SET LEGEND.RX; RUN;

PROC EXPORT
		DATA=RX
		OUTFILE='T:\SAS\RX.XLSX'
		REPLACE;
RUN;
