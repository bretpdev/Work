DATA SSNS;
	INFILE 'T:\SAS\KU & KI OID errors needing fixed.csv'
	DELIMITER = ',';

	LENGTH BF_SSN $X.;
	INPUT BF_SSN $ LN_SEQ;
RUN;

LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;

DATA LEGEND.SSNS; SET SSNS; RUN;

RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE MRXX AS
		SELECT
			MRXX.*
		FROM
			SSNS
			JOIN PKUB.MRXX_ORG_ISS_DSU MRXX
				ON SSNS.BF_SSN = MRXX.BF_SSN
/*				AND SSNS.LN_SEQ = MRXX.LN_SEQ*/
	;
QUIT;

ENDRSUBMIT;

DATA MRXX; SET LEGEND.MRXX; RUN;

PROC EXPORT
		DATA=MRXX
		OUTFILE='T:\SAS\DATA DUMP MRXX_ORG_ISS_DSU.XLSX'
		REPLACE;
RUN;
