LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=work  ;
RSUBMIT LEGEND;
/*%let DB = DNFPRQUT;  *This is test;*/
/*%let DB = DNFPRUUT;  *This is VUKX test;*/
%let DB = DNFPUTDL;  *This is live;

LIBNAME PKUB DBX DATABASE=&DB OWNER=PKUB;

PROC SQL;
	CONNECT TO DBX (DATABASE=&DB);

	CREATE TABLE DEMO AS
		SELECT	
			*
		FROM	
			CONNECTION TO DBX 
				(
					SELECT DISTINCT
						PDXX.DF_SPE_ACC_ID,
						PDXX.DM_PRS_LST,
						LNXX.LF_DOE_SCL_ORG,
						SDXX.IF_DOE_SCL
					FROM
						PKUB.PDXX_PRS_NME PDXX
					LEFT OUTER JOIN PKUB.LNXX_LON LNXX
						ON LNXX.BF_SSN = PDXX.DF_PRS_ID
						AND LNXX.LF_DOE_SCL_ORG like 'XXXXXX%'
					LEFT OUTER JOIN PKUB.SDXX_STU_ENR SDXX
						ON SDXX.LF_STU_SSN = PDXX.DF_PRS_ID
						AND SDXX.LC_STA_SDXX = 'A'
						AND SDXX.IF_DOE_SCL like 'XXXXXX%'
					WHERE 
						SDXX.IF_DOE_SCL like 'XXXXXX%'
						OR LNXX.LF_DOE_SCL_ORG like 'XXXXXX%'
						
			FOR READ ONLY WITH UR
				)
	;

	DISCONNECT FROM DBX;

	/*%PUT  SQLXRC= >>> &SQLXRC <<< ||| SQLXMSG= >>> &SQLXMSG >>> ;  ** INCLUDES ERROR MESSAGES TO SAS LOG  ;*/
	/*%SQLCHECK;*/
QUIT;

ENDRSUBMIT;

DATA DEMO; SET LEGEND.DEMO; RUN;

PROC EXPORT DATA = DEMO 
            OUTFILE = "T:\SAS\NH XXXX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="OUTPUTX"; 
RUN;
