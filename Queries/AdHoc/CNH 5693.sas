LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE WQXX AS
		SELECT
			WQXX.WF_QUE,
			WQXX.WF_SUB_QUE,
			WQXX.WN_CTL_TSK,
			WQXX.PF_REQ_ACT
		FROM
			PKUB.WQXX_TSK_QUE WQXX
		WHERE
			WQXX.WF_QUE = 'RX'
			AND WQXX.WF_SUB_QUE = 'XX'
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA = LEGEND.WQXX
		OUTFILE='T:\SAS\CornerStone Queue Task Killer Query.XLSX'
		REPLACE;
RUN;
