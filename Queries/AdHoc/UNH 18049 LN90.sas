
LIBNAME  DUSTER  REMOTE  SERVER=DUSTER  SLIBREF=WORK;
RSUBMIT;

PROC SQL;
	CONNECT TO DB2 (DATABASE=DLGSUTWH);

	CREATE TABLE DEMO AS
		SELECT 
			*
		FROM 
			CONNECTION TO DB2 
				(
					SELECT
						LN90.*
					FROM	
						OLWHRM1.LN90_FIN_ATY LN90
					WHERE	
						LN90.PC_FAT_TYP IN ('52','05')
						OR
						LN90.PC_FAT_SUB_TYP IN ('24', '14', '51', '52')

					FOR READ ONLY WITH UR
				)
	;
	DISCONNECT FROM DB2;
QUIT;
ENDRSUBMIT;
