LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=work  ;
RSUBMIT LEGEND;
%let DB = DNFPRQUT;  *This is test;

LIBNAME PKUB DBX DATABASE=&DB OWNER=PKUB;

PROC SQL;
	CONNECT TO DBX (DATABASE=&DB);

	CREATE TABLE AYXX AS
		SELECT	
			*
		FROM	
			CONNECTION TO DBX 
				(
					SELECT	
						AYXX.*
					FROM
						PKUB.PDXX_PRS_NME PDXX
						INNER JOIN PKUB.AYXX_BR_LON_ATY AYXX
							ON PDXX.DF_PRS_ID = AYXX.BF_SSN
					WHERE
						PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'

					FOR READ ONLY WITH UR
				)
	;

		CREATE TABLE AYXX AS
		SELECT	
			*
		FROM	
			CONNECTION TO DBX 
				(
					SELECT	
						AYXX.*
					FROM
						PKUB.PDXX_PRS_NME PDXX
						INNER JOIN PKUB.AYXX_ATY_CMT AYXX
							ON PDXX.DF_PRS_ID = AYXX.BF_SSN
					WHERE
						PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'

					FOR READ ONLY WITH UR
				)
	;

		CREATE TABLE LNXX AS
		SELECT	
			*
		FROM	
			CONNECTION TO DBX 
				(
					SELECT	
						LNXX.*
					FROM
						PKUB.PDXX_PRS_NME PDXX
						INNER JOIN PKUB.LNXX_LON_ATY LNXX
							ON PDXX.DF_PRS_ID = LNXX.BF_SSN
					WHERE
						PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'

					FOR READ ONLY WITH UR
				)
	;

	DISCONNECT FROM DBX;

QUIT;

ENDRSUBMIT;
DATA AYXX; SET LEGEND.AYXX; RUN;
DATA AYXX; SET LEGEND.AYXX; RUN;
DATA LNXX; SET LEGEND.LNXX; RUN;


/*export to Excel spreadsheet*/
PROC EXPORT DATA= WORK.AYXX
            OUTFILE= "T:\SAS\Data Dump to Assist with SAS for DMCS.xls" 
            DBMS=EXCEL REPLACE;
     SHEET="AYXX"; 
RUN;

PROC EXPORT DATA= WORK.AYXX
            OUTFILE= "T:\SAS\Data Dump to Assist with SAS for DMCS.xls" 
            DBMS=EXCEL REPLACE;
     SHEET="AYXX"; 
RUN;

PROC EXPORT DATA= WORK.LNXX
