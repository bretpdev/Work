							AND AYXX.LN_ATY_SEQ = AYXXX.LN_ATY_SEQ
							AND AYXXX.LN_ATY_CMT_SEQ = 'X'
						LEFT OUTER JOIN PKUB.AYXX_ATY_TXT AYXXX
