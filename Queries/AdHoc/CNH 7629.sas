						CASE WHEN AYXX.PF_REQ_ACT = 'PXXXC' THEN 'Email'
						     WHEN AYXX.PF_REQ_ACT = 'PXXXA' THEN 'Note'
							 ELSE 'Phone' END AS Type,
						CONCAT(AYXX.PF_REQ_ACT,PX_ACT_DSC_REQ) AS Reason,
						CONCAT(CONCAT(CONCAT(CONCAT(COALESCE(AYXX.LX_ATY,''),COALESCE(AYXXX.LX_ATY,' ')),COALESCE(AYXXX.LX_ATY,' ')),COALESCE(AYXXX.LX_ATY,' ')),COALESCE(AYXXX.LX_ATY,' ')) AS Description,
