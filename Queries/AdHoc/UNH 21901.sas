LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LN10 AS
		SELECT
			PD10.DF_SPE_ACC_ID,
			LN10.BF_SSN,
			LN10.LN_SEQ,
			LN10.LD_EFF_LBR_RTE
		FROM
			OLWHRM1.LN10_LON LN10
			JOIN OLWHRM1.PD10_PRS_NME PD10
				ON LN10.BF_SSN = PD10.DF_PRS_ID
		WHERE
			(PD10.DF_SPE_ACC_ID = '3767591747' AND LN10.LN_SEQ IN (14,16))
			OR (PD10.DF_SPE_ACC_ID = '4763238664' AND LN10.LN_SEQ IN (8,10))
			OR (PD10.DF_SPE_ACC_ID = '4797717699' AND LN10.LN_SEQ IN (10,12))
			OR (PD10.DF_SPE_ACC_ID = '9920939765' AND LN10.LN_SEQ IN (11,15))
			OR (PD10.DF_SPE_ACC_ID = '5347718218' AND LN10.LN_SEQ IN (14,16))
			OR (PD10.DF_SPE_ACC_ID = '6469202007' AND LN10.LN_SEQ IN (7,9))
	;
QUIT;
ENDRSUBMIT;

DATA LN10; SET DUSTER.LN10; RUN;

PROC EXPORT	
		DATA=LN10
		OUTFILE='T:\Data Dump of Missing LIBOR Dates.XLSX'
		REPLACE;
RUN;	
