LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;


PROC SQL;
	
	CREATE TABLE DELQXXX AS
		SELECT PDXX.DF_SPE_ACC_ID As AccountNumber, 
		LNXX.LN_SEQ as LoanSequence,
		LNXX.LD_DLQ_OCC+XXX AS DateXXXDaysDelinquent
		FROM PKUB.PDXX_PRS_NME PDXX
			INNER JOIN PKUB.LNXX_LON LNXX
				ON PDXX.DF_PRS_ID = LNXX.BF_SSN
			INNER JOIN PKUB.LNXX_LON_DLQ_HST LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN AND LNXX.LN_SEQ = LNXX.LN_SEQ
		WHERE LNXX.LN_DLQ_MAX >= XXX
			  AND (LNXX.LD_DLQ_OCC+XXX) BETWEEN 'XXJANXXXX'd AND TODAY()
	;
QUIT;

ENDRSUBMIT;

DATA DELQXXX; SET LEGEND.DELQXXX; RUN;

PROC EXPORT	
		DATA=LEGEND.DELQXXX
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;
