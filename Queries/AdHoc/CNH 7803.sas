LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;


PROC SQL;
	
	CREATE TABLE LNXXDUMP AS
		SELECT LNXX.*
		FROM
			PKUB.LNXX_LON_RPS LNXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_PRS_ID = LNXX.BF_SSN
		WHERE 
		PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE RSXXDUMP AS
		SELECT RSXX.*
		FROM
			PKUB.RSXX_IBR_RPS RSXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE 
		PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE RSXXDUMP AS
		SELECT RSXX.*
		FROM
			PKUB.RSXX_BR_RPD RSXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE 
		PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;

	CREATE TABLE RSXXDUMP AS
		SELECT RSXX.*
		FROM
			PKUB.RSXX_IBR_IRL_LON RSXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON PDXX.DF_PRS_ID = RSXX.BF_SSN
		WHERE 
		PDXX.DF_SPE_ACC_ID = 'XXXXXXXXXX'
	;
QUIT;

ENDRSUBMIT;

DATA LNXXDUMP; SET LEGEND.LNXXDUMP; RUN;

PROC EXPORT	
		DATA=LEGEND.LNXXDUMP
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;

DATA RSXXDUMP; SET LEGEND.RSXXDUMP; RUN;

PROC EXPORT	
		DATA=LEGEND.RSXXDUMP
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;

DATA RSXXDUMP; SET LEGEND.RSXXDUMP; RUN;

PROC EXPORT	
		DATA=LEGEND.RSXXDUMP
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;

DATA RSXXDUMP; SET LEGEND.RSXXDUMP; RUN;

PROC EXPORT	
		DATA=LEGEND.RSXXDUMP
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;
