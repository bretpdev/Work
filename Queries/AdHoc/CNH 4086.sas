LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;

PROC SQL;
	CREATE TABLE FICR AS
		SELECT DISTINCT
   			PDXX.DF_SPE_ACC_ID,
   			PDXX.DM_PRS_LST,
   			LNXX.LC_TYP_SCH_DIS,
   			LNXX.LI_FRC_IBR,
   			FSXX.LI_FRC_ICR 
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.LNXX_LON_RPS LNXX
				ON PDXX.DF_PRS_ID = LNXX.BF_SSN
			JOIN PKUB.FSXX_DL_LON FSXX
				ON LNXX.BF_SSN = FSXX.BF_SSN
				AND LNXX.LN_SEQ = FSXX.LN_SEQ
			JOIN PKUB.LNXX_LON LNXX
				ON LNXX.BF_SSN = LNXX.BF_SSN
				AND LNXX.LN_SEQ = LNXX.LN_SEQ
		WHERE
			LNXX.LA_CUR_PRI > X
   			AND LNXX.LC_TYP_SCH_DIS IN ('CX', 'CX', 'CX', 'CQ', 'IL', 'IB')
   			AND 
				(
					FSXX.LI_FRC_ICR = 'Y'  
					OR
					LNXX.LI_FRC_IBR = 'Y'
				)
	;
				
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.FICR
		OUTFILE='T:\SAS\NHCS XXXX.XLSX'
		REPLACE;
RUN;
