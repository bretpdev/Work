LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;


PROC SQL;
	
	CREATE TABLE MRXXData AS
		SELECT PDXX.DF_SPE_ACC_ID, 
		PDXX.DF_PRS_ID,
		MRXX.*
		FROM
			PKUB.MRXX_MSC_TAX_RPT MRXX
			INNER JOIN PKUB.PDXX_PRS_NME PDXX
			ON MRXX.BF_SSN = PDXX.DF_PRS_ID
		WHERE MRXX.LF_TAX_YR = 'XXXX'
	;
QUIT;

ENDRSUBMIT;

DATA MRXXData; SET LEGEND.MRXXData; RUN;

PROC EXPORT	
		DATA=LEGEND.MRXXData
		OUTFILE='T:\SAS\NH XXXX data dump.xlsx'
		REPLACE;
RUN;
