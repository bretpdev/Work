PROC IMPORT OUT= WORK.DLOS
            DATAFILE= "T:\Master.xlsx" 
            DBMS=EXCEL REPLACE;
     RANGE="DLO$"; 
     GETNAMES=YES;
     MIXED=NO;
     SCANTEXT=YES;
     USEDATE=YES;
     SCANTIME=YES;
RUN;

PROC IMPORT OUT= WORK.LNCS
            DATAFILE= "T:\Master.xlsx" 
            DBMS=EXCEL REPLACE;
     RANGE="LNC$"; 
     GETNAMES=YES;
     MIXED=NO;
     SCANTEXT=YES;
     USEDATE=YES;
     SCANTIME=YES;
RUN;

DATA DLOS;
      SET DLOS;
      BF_SSN = STRIP(COMPRESS(SSN,'-'));
      FORMAT BF_SSN $X.;
	  DROP AWARD_ID LD_LON_GTR;
RUN;

DATA LNCS;
      SET LNCS;
      BF_SSN = STRIP(COMPRESS(SSN,'-'));
      FORMAT BF_SSN $X.;
	  DROP AWARD_ID LD_LON_GTR;
RUN;

/*%LET RPTLIB = %SYSGET(reportdir);*/
%LET RPTLIB = T:\SAS;
FILENAME REPORTZ "&RPTLIB/UNWSXX.NWSXXRZ";
FILENAME REPORTX "&RPTLIB/UNWSXX.NWSXXRX";

LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=work  ;

DATA LEGEND.DLOS;
	SET DLOS;
RUN;

DATA LEGEND.LNCS;
	SET LNCS;
RUN;

RSUBMIT LEGEND;
/*%let DB = DNFPRQUT;  *This is test;*/
/*%let DB = DNFPRUUT;  *This is VUKX test;*/
%let DB = DNFPUTDL;  *This is live;

LIBNAME PKUB DBX DATABASE=&DB OWNER=PKUB;

PROC SQL ;
	CONNECT TO DBX (DATABASE=&DB);

	CREATE TABLE DLO AS
		SELECT	
			D.*
			,LNXX.LD_LON_GTR
			,FSXX.LF_FED_AWD || PUT(FSXX.LN_FED_AWD_SEQ,ZX.) AS AWARD_ID
		FROM	
			DLOS D
			INNER JOIN PKUB.LNXX_LON LNXX
				ON D.BF_SSN = LNXX.BF_SSN
			LEFT JOIN PKUB.FSXX_DL_LON FSXX
				ON D.BF_SSN = FSXX.BF_SSN
				AND LNXX.LN_SEQ = FSXX.LN_SEQ
	;

	CREATE TABLE LNC AS
		SELECT	
			L.*
			,LNXX.LD_LON_GTR
			,FSXX.LF_FED_AWD || PUT(FSXX.LN_FED_AWD_SEQ,ZX.) AS AWARD_ID
		FROM	
			LNCS L
			INNER JOIN PKUB.LNXX_LON LNXX
				ON L.BF_SSN = LNXX.BF_SSN
			LEFT JOIN PKUB.FSXX_DL_LON FSXX
				ON L.BF_SSN = FSXX.BF_SSN
				AND LNXX.LN_SEQ = FSXX.LN_SEQ
	;

	DISCONNECT FROM DBX;

	/*%PUT  SQLXRC= >>> &SQLXRC <<< ||| SQLXMSG= >>> &SQLXMSG >>> ;  ** INCLUDES ERROR MESSAGES TO SAS LOG  ;*/
	/*%SQLCHECK;*/
QUIT;

ENDRSUBMIT;

DATA DLO; SET LEGEND.DLO; RUN;
DATA LNC; SET LEGEND.LNC; RUN;

DATA DLO;
	SET DLO;
	DROP BF_SSN;
RUN;

DATA LNC; 
	SET LNC;
	DROP BF_SSN;
RUN;

/*export to Excel spreadsheet*/
PROC EXPORT DATA = WORK.DLO
            OUTFILE = "T:\SAS\NH XXXX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="DLO"; 
RUN;

/*export to Excel spreadsheet*/
PROC EXPORT DATA = WORK.LNC
            OUTFILE = "T:\SAS\NH XXXX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="LNC"; 
RUN;


