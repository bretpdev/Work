PROC IMPORT OUT= WORK.SOURCE
            DATAFILE= "T:\Award IDs.xlsx" 
            DBMS=EXCEL REPLACE;
     RANGE="SheetX$"; 
     GETNAMES=YES;
     MIXED=NO;
     SCANTEXT=YES;
     USEDATE=YES;
     SCANTIME=YES;
RUN;

DATA LEGEND.SOURCE;
SET SOURCE;
RUN;



LIBNAME  LEGEND  REMOTE  SERVER=LEGEND SLIBREF=work  ;
RSUBMIT LEGEND;
/*%let DB = DNFPRQUT;  *This is test;*/
/*%let DB = DNFPRUUT;  *This is VUKX test;*/
%let DB = DNFPUTDL;  *This is live;

LIBNAME PKUB DBX DATABASE=&DB OWNER=PKUB;
PROC SQL;
	CREATE TABLE DEMO AS
					SELECT DISTINCT
						FSXX.BF_SSN,
						FSXX.LN_SEQ,
						S.OLDAWARDID,
						S.NEW_AWARD_ID
					FROM
						PKUB.FSXX_DL_LON FSXX
						INNER JOIN SOURCE S
							ON S.OLDAWARDID = (FSXX.LF_FED_AWD || PUT(FSXX.LN_FED_AWD_SEQ, ZX.))
				
;
QUIT;

ENDRSUBMIT;

DATA DEMO; SET LEGEND.DEMO; RUN;

PROC EXPORT DATA = WORK.DEMO 
            OUTFILE = "T:\NH XXXX.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="SheetX"; 
RUN;
