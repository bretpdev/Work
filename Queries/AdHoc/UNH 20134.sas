LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;

	CREATE TABLE DELQ AS
		SELECT DISTINCT
			LN10.BF_SSN
			,LN10.LN_SEQ
			,LN16.LN_DLQ_MAX
		FROM
			OLWHRM1.LN10_LON LN10
			INNER JOIN OLWHRM1.LN16_LON_DLQ_HST LN16
				ON LN10.BF_SSN = LN16.BF_SSN
				AND LN10.LN_SEQ = LN16.LN_SEQ
		WHERE
			LN10.IF_GTR IN ('000706', '000708', '000730', '000731', '000751', '000755', '000800', '000927', '000951')
			AND LN10.LA_CUR_PRI > 0
			AND LN10.LC_STA_LON10 = 'R'
			AND LN16.LC_STA_LON16 = '1'
			AND LN16.LN_DLQ_MAX > 301
	;

	CREATE TABLE BNK AS
		SELECT DISTINCT
			LN10.BF_SSN
			,PD24.DC_BKR_TYP
		FROM
			OLWHRM1.LN10_LON LN10
			INNER JOIN OLWHRM1.PD24_PRS_BKR PD24
				ON LN10.BF_SSN = PD24.DF_PRS_ID
		WHERE
			LN10.IF_GTR IN ('000706', '000708', '000730', '000731', '000751', '000755', '000800', '000927', '000951')
			AND LN10.LA_CUR_PRI > 0
			AND LN10.LC_STA_LON10 = 'R'
			AND PD24.DC_BKR_TYP = '13'
	;

QUIT;

ENDRSUBMIT;

DATA DELQ; SET DUSTER.DELQ; RUN;
DATA BNK; SET DUSTER.BNK; RUN;

PROC EXPORT
		DATA=DELQ
		OUTFILE='T:\SAS\Non-UHEAA Query for Claims.XLSX'
		REPLACE;
RUN;

PROC EXPORT
		DATA=BNK
		OUTFILE='T:\SAS\Non-UHEAA Query for Claims.XLSX'
		REPLACE;
RUN;
