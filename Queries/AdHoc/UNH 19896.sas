LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE LP06 AS
		SELECT
			A.*
		FROM
			OLWHRM1.LP06_ITR_AND_TYP A
			LEFT JOIN OLWHRM1.LP06_ITR_AND_TYP B
				ON A.PC_STA_LPD06 = B.PC_STA_LPD06
				AND A.IC_LON_PGM = B.IC_LON_PGM
				AND A.PF_RGL_CAT = B.PF_RGL_CAT
				AND A.IF_GTR = B.IF_GTR
				AND A.IF_OWN = B.IF_OWN
				AND A.PC_ITR_TYP = B.PC_ITR_TYP
				AND A.PD_EFF_SR_LPD06 = B.PD_EFF_SR_LPD06
				AND B.IF_BND_ISS = '14002014'
		WHERE
			A.PC_STA_LPD06 = 'A'
			AND A.IF_GTR IN ('000706', '000708', '000730', '000731', '000751', '000755', '000800', '000951')
			AND A.IF_BND_ISS = '00000000'
			AND B.PC_STA_LPD06 IS NULL
	;
QUIT;
ENDRSUBMIT;

DATA LP06; SET DUSTER.LP06; RUN;
			
PROC EXPORT
		DATA=LP06
		OUTFILE='T:\SAS\Interest Rate LPDs for Wyo Loans.XLSX'
		REPLACE;
RUN;
