LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;
PROC SQL;
	CREATE TABLE SCRA AS
		SELECT
			LN72.BF_SSN,
			PD10.DM_PRS_LST,
			LN72.LN_SEQ,
			LN72.LR_ITR,
			LN72.LC_INT_RDC_PGM,
			LN72.LD_CRT_LON72,
			LN72.LD_ITR_APL,
			LN72.LD_ITR_EFF_BEG,
			LN72.LD_ITR_EFF_END
		FROM
			OLWHRM1.PD10_PRS_NME PD10
			JOIN OLWHRM1.LN72_INT_RTE_HST LN72
				ON PD10.DF_PRS_ID = LN72.BF_SSN
		WHERE
			PD10.DF_SPE_ACC_ID IN ('4044644426','4959325920','6222750024')
			AND LN72.LC_INT_RDC_PGM IN ('M','P')
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.SCRA
		OUTFILE='T:\NH 22034.XLSX'
		REPLACE;
RUN;
