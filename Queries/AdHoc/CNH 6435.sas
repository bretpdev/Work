LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;
RSUBMIT;
LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE RX AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			AYXX.LD_ATY_REQ_RCV
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.AYXX_BR_LON_ATY AYXX
				ON PDXX.DF_PRS_ID = AYXX.BF_SSN
		WHERE
			AYXX.PF_REQ_ACT = 'DFDNY'
			AND AYXX.LC_STA_ACTYXX = 'A'
			AND AYXX.LD_ATY_REQ_RCV >= 'XXMARXXXX'D
	;

	CREATE TABLE RX AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			LNXX.LD_DFR_APL,
			DFXX.LC_DFR_TYP
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.DFXX_BR_DFR_REQ DFXX
				ON PDXX.DF_PRS_ID = DFXX.BF_SSN
			JOIN PKUB.LNXX_BR_DFR_APV LNXX
				ON DFXX.BF_SSN = LNXX.BF_SSN
				AND DFXX.LF_DFR_CTL_NUM = LNXX.LF_DFR_CTL_NUM
		WHERE
			DFXX.LC_DFR_STA = 'A'
			AND DFXX.LC_STA_DFRXX = 'A'
			AND LNXX.LC_STA_LONXX = 'A'
			AND LNXX.LD_DFR_APL >= 'XXMARXXXX'D
	;

	CREATE TABLE RX AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			AYXX.LD_ATY_REQ_RCV
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.AYXX_BR_LON_ATY AYXX
				ON PDXX.DF_PRS_ID = AYXX.BF_SSN
		WHERE
			AYXX.PF_REQ_ACT = 'FBDNY'
			AND AYXX.LC_STA_ACTYXX = 'A'
			AND AYXX.LD_ATY_REQ_RCV >= 'XXMARXXXX'D
	;

	CREATE TABLE RX AS
		SELECT DISTINCT
			PDXX.DF_SPE_ACC_ID,
			LNXX.LD_FOR_APL,
			FBXX.LC_FOR_TYP			
		FROM
			PKUB.PDXX_PRS_NME PDXX
			JOIN PKUB.FBXX_BR_FOR_REQ FBXX
				ON PDXX.DF_PRS_ID = FBXX.BF_SSN
			JOIN PKUB.LNXX_BR_FOR_APV LNXX
				ON FBXX.BF_SSN = LNXX.BF_SSN
				AND FBXX.LF_FOR_CTL_NUM = LNXX.LF_FOR_CTL_NUM
		WHERE
			FBXX.LC_FOR_STA = 'A'
			AND FBXX.LC_STA_FORXX = 'A'
			AND LNXX.LC_STA_LONXX = 'A'
			AND LNXX.LD_FOR_APL >= 'XXMARXXXX'D
	;
QUIT;
ENDRSUBMIT;

