LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE TAXES AS
		SELECT DISTINCT
			LN90.BF_SSN,
			LN90.LD_FAT_EFF,
			SUM(LN90.LA_FAT_ILG_PRI) AS LA_FAT_ILG_PRI,
			SUM(LN90.LA_FAT_CUR_PRI) AS LA_FAT_CUR_PRI
		FROM
			OLWHRM1.LN90_FIN_ATY LN90
		WHERE
			LN90.PC_FAT_TYP = '50' 
			AND LN90.PC_FAT_SUB_TYP = '02' 
			AND LN90.LC_FAT_REV_REA = ' '
			AND LN90.LD_FAT_EFF > '01JAN2011'D
			AND LN90.LC_STA_LON90 = 'A'
		GROUP BY
			LN90.BF_SSN,
			LN90.LD_FAT_EFF
	;
QUIT;

ENDRSUBMIT;

PROC EXPORT
		DATA=DUSTER.TAXES
		OUTFILE='T:\TAXES.XLSX'
		REPLACE;
RUN;

