LIBNAME XL 'T:\CornerStone Test Borrowers on ACH.xlsx';
LIBNAME LEGEND REMOTE SERVER=LEGEND SLIBREF=WORK;

DATA LEGEND.SSNS;
	SET XL.'SHEETX$'N;
RUN;

RSUBMIT;
LIBNAME WEBFLSX DBX DATABASE=DNFPUTDL OWNER=WEBFLSX;
PROC SQL;
	CREATE TABLE WBXX AS
		SELECT
			SSNS.SSN
		FROM
			SSNS
			JOIN WEBFLSX.WBXX_CSM_USR_ACC WBXX
				ON SSNS.SSN = WBXX.DF_USR_SSN
		ORDER BY 
			SSNS.SSN
	;
QUIT;
ENDRSUBMIT;

PROC EXPORT
		DATA=LEGEND.WBXX
		OUTFILE='T:\CS Test Borrowers on ACH with Web Accouts.XLSX'
		REPLACE;
RUN;
