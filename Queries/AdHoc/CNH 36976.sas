LIBNAME PKUB DBX DATABASE=DNFPUTDL OWNER=PKUB;
PROC SQL;
	CREATE TABLE FSXX_XXXXX AS
		SELECT DISTINCT
			 F.BF_SSN
			,F.LN_SEQ
			,F.Old_LI_FRC_ICR
			,F.New_LI_FRC_ICR
			,F.Old_LI_FRC_PAYE
			,F.New_LI_FRC_PAYE
			,F.Old_LI_FRC_RPYE
			,F.New_LI_FRC_RPYE
			,FSXX.LI_FRC_ICR
			,FSXX.LI_FRC_PAYE
			,FSXX.LI_FRC_RPYE
		FROM
			_FSXX_XXXXX F
			LEFT JOIN PKUB.FSXX_DL_LON FSXX
				ON F.BF_SSN = FSXX.BF_SSN
				AND F.LN_SEQ = FSXX.LN_SEQ
	;
	CREATE TABLE FSXX_XXXXX AS
		SELECT DISTINCT
			 F.BF_SSN
			,F.LN_SEQ
			,F.Old_LI_FRC_ICR
			,F.New_LI_FRC_ICR
			,F.Old_LI_FRC_PAYE
			,F.New_LI_FRC_PAYE
			,F.Old_LI_FRC_RPYE
			,F.New_LI_FRC_RPYE
			,FSXX.LI_FRC_ICR
			,FSXX.LI_FRC_PAYE
			,FSXX.LI_FRC_RPYE
		FROM
			_FSXX_XXXXX F
			LEFT JOIN PKUB.FSXX_DL_LON FSXX
				ON F.BF_SSN = FSXX.BF_SSN
				AND F.LN_SEQ = FSXX.LN_SEQ
	;
QUIT;
ENDRSUBMIT;

