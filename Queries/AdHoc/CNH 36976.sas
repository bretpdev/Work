	SET XL.'FSXX_XXXXX$'N (DROP = FX);
	SSN=PUT(BF_SSN, zX.);
	DROP BF_SSN;
RUN;
DATA _FSXX_XXXXX (RENAME=(SSN=BF_SSN));
	SET XL.'FSXX_XXXXX$'N (DROP = FX FXX FXX FXX FXX FXX FXX FXX);
	SSN=PUT(BF_SSN, zX.);
