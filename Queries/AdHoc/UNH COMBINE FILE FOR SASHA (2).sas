PROC IMPORT OUT = WORK.SASHAFILE
            DATAFILE = "T:\KPMG Sample Accounts.xls"
			REPLACE;
            SHEET = 'Sheet2$'n; 
RUN;

PROC IMPORT OUT = WORK.JAROMFILE
            DATAFILE = "C:\SERF_File\Repayment Audit Query 07-10-2014-v3.xlsx" 
			REPLACE;
            SHEET = 'Sheet1$'n; 
RUN;

PROC SQL;
	CREATE TABLE FINAL AS 
		SELECT DISTINCT
			S.SSN,
			S.LN_SEQ,
			S.REPAY_ELAPSED_MTHS,
			S.REPAY_REMAINING_MTHS,
			J.IF_BND_ISS,
			J.GRACE_END,
			J.LD_LON_1_DSB,
			J.LD_END_GRC_PRD,
			J.PN_RPD_TRM_MAX,
			J.REPAYMENT_END_DATE,
			J.DEFERMENT_MONTH_USED,
			J.TOTAL_DEFERMENT_MONTH_USED,
			J.FORBEARANCE_MONTH_USED,
			J.TOTAL_FORBEARANCE_MONTH_USED,
			J.NEW_REPAYMENT_END_DATE,
			J.TERMS_REMAINING,
			J.PAYMENTS_MADE,
			S.F16 AS SASHAS_NOTE
		FROM SASHAFILE S
			INNER JOIN JAROMFILE J
				ON S.SSN = J.BF_SSN
				AND S.LN_SEQ = J.LN_SEQ
		
;
QUIT;
			
PROC EXPORT DATA = WORK.FINAL 
            OUTFILE = "T:\KPMG SAMPLE ACCOUNT MERGED FILE.xlsx" 
            DBMS = EXCEL
			REPLACE;
     SHEET="Sheet1"; 
RUN;
