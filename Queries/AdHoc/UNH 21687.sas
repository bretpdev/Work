LIBNAME DUSTER REMOTE SERVER=DUSTER SLIBREF=WORK;
RSUBMIT;
LIBNAME OLWHRM1 DB2 DATABASE=DLGSUTWH OWNER=OLWHRM1;

PROC SQL;
	CREATE TABLE TEN99 AS
		SELECT
			*
		FROM
			OLWHRM1.LN90_FIN_ATY LN90
		WHERE
			LD_FAT_EFF >= '01JAN2014'D
		HAVING
			CATX('/',PC_FAT_TYP,PC_FAT_SUB_TYP) IN 
				(
					'10/78',
					'15/06',
					'15/08',
					'17/06',
					'17/08',
					'18/06',
					'18/08',
					'19/08',
					'50/02',
					'51/06',
					'51/08',
					'52/01',
					'55/02'
				)
	;
QUIT;
ENDRSUBMIT;

DATA TEN99; SET DUSTER.TEN99; RUN;

PROC EXPORT
		DATA=TEN99
		OUTFILE='T:\1099 transactions.XLSX'
		REPLACE;
RUN;
