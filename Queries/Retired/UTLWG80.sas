PROC SQL ;
CONNECT TO DB2 (DATABASE=DLGSUTWH);
CREATE TABLE POS AS
SELECT *
FROM CONNECTION TO DB2 (
SELECT A.DF_PRS_ID_BR
	,RTRIM(B.AF_APL_ID)||B.AF_APL_ID_SFX AS CLUID
	,B.AD_PRC 
	,C.AD_DSB_ADJ
	,B.AC_PRC_STA 
	,C.AC_DSB_ADJ
	,C.AN_DSB_SEQ 
FROM OLWHRM1.GA01_APP A
INNER JOIN OLWHRM1.GA10_LON_APP B
	ON A.AF_APL_ID = B.AF_APL_ID
INNER JOIN OLWHRM1.GA11_LON_DSB_ATY C 
	ON B.AF_APL_ID = C.AF_APL_ID
	AND B.AF_APL_ID_SFX = C.AF_APL_ID_SFX
INNER JOIN OLWHRM1.GA14_LON_STA D
	ON D.AF_APL_ID = B.AF_APL_ID
	AND D.AF_APL_ID_SFX = B.AF_APL_ID_SFX
WHERE B.AC_PRC_STA = 'A'
AND C.AC_DSB_ADJ_STA = 'A'
AND C.AC_DSB_ADJ = 'E'
AND C.AN_DSB_SEQ = 1
AND C.AD_DSB_ADJ >= &FYR
AND D.AC_LON_STA_TYP <> 'CA'
);

CREATE TABLE ACHK AS
SELECT *
FROM CONNECTION TO DB2 (
SELECT DF_PRS_ID
	,BX_CMT
FROM OLWHRM1.AY01_BR_ATY 
WHERE PF_ACT = 'GDSST'
AND BD_ATY_PRF > '01/01/2003'
);
DISCONNECT FROM DB2;
/*ENDRSUBMIT;*/
/*DATA POS; SET WORKLOCL.POS; RUN;*/
/*DATA ACHK; SET WORKLOCL.ACHK; RUN;*/
PROC SORT DATA=POS NODUPREC;
BY DF_PRS_ID_BR CLUID AN_DSB_SEQ ;
RUN;
PROC SORT DATA=ACHK;
BY DF_PRS_ID;
RUN;

DATA ACHK;
SET ACHK;
X = LENGTH(BX_CMT);
RUN;

/*USED IN DEVELOPMENT*/
/*PROC SQL;*/
/*CREATE TABLE X AS */
/*SELECT **/
/*FROM ACHK*/
/*WHERE X NOT IN (76,95,97);*/
/*QUIT;*/
/*RUN;*/

DATA ACHK;
SET ACHK;
WHERE X IN (76,95,97,125);
RUN;

DATA ACHK76;
SET ACHK;
WHERE X = 76;
RUN;

DATA ACHK76;
SET ACHK76;
LENGTH CACHK $ 1;
IF LENGTH(BX_CMT) = 76 THEN CACHK = SUBSTR(BX_CMT,32,1);
RUN;

DATA ACHK76;
SET ACHK76;
LENGTH C1 $ 19;
IF CACHK = ' ' THEN C1 = SUBSTR(BX_CMT,33,19);
ELSE IF CACHK = '0' THEN C1 = SUBSTR(BX_CMT,32,19);
RUN;

DATA ACHK1;
SET ACHK;
LENGTH C1 $ 19;
IF LENGTH(BX_CMT) = 76 THEN DELETE;
ELSE IF LENGTH(BX_CMT) = 95 THEN C1 = SUBSTR(BX_CMT,32,19);
ELSE IF LENGTH(BX_CMT) = 97 THEN C1 = SUBSTR(BX_CMT,54,19);
ELSE IF LENGTH(BX_CMT) = 125 THEN C1 = SUBSTR(BX_CMT,32,19); 
RUN;

DATA ACHK1 (DROP=CACHK);
SET ACHK76 ACHK1;
RUN;

DATA ACHK1;
SET ACHK1;
LENGTH C2 $ 19;
IF LENGTH(BX_CMT) = 76 THEN C2 = 'NONE';
ELSE IF LENGTH(BX_CMT) = 95 THEN C2 = SUBSTR(BX_CMT,52,19);
ELSE IF LENGTH(BX_CMT) = 97 THEN C2 = 'NONE ';
RUN;

DATA ACHKCONT (DROP= BX_CMT DF_PRS_ID X);
SET ACHK1;
DF_PRS_ID_BR=DF_PRS_ID;
RUN;

PROC SORT DATA=ACHKCONT;
BY DF_PRS_ID_BR;
RUN;

DATA CID1 (DROP=C2 C1);
SET ACHKCONT;
CLUID=C1;
RUN;

DATA CID2 (DROP=C1 C2);
SET ACHKCONT;
CLUID=C2;
RUN;

DATA CID2;
SET CID2;
IF CLUID = 'NONE' THEN DELETE;
RUN;

PROC SORT DATA=CID1;
BY DF_PRS_ID_BR CLUID;
RUN;

PROC SORT DATA=CID2;
BY DF_PRS_ID_BR CLUID;
RUN;

PROC SORT DATA=POS;
BY DF_PRS_ID_BR CLUID;
RUN;

DATA BOTH DERR GRB1;
MERGE POS (IN=A) CID1(IN=B);
BY DF_PRS_ID_BR CLUID;
IF A AND B THEN OUTPUT BOTH;
ELSE IF A THEN OUTPUT DERR;
ELSE IF B THEN OUTPUT GRB1;
RUN;

/*=====================================================================================*/
/*DERR2 IS THE "MASTER" DATASET. IT CONTAINS ALL THE LOANS THAT NEED TO BE DISCLOSED ON*/
/*=====================================================================================*/

DATA BOTH2 DERR2 GRB2;
MERGE DERR (IN=A) CID2(IN=B);
BY DF_PRS_ID_BR CLUID;
IF A AND B THEN OUTPUT BOTH2;
ELSE IF A THEN OUTPUT DERR2;
ELSE IF B THEN OUTPUT GRB2;
RUN;
