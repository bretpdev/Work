  <DataSets>
    <DataSet Name="NeedHelpDailyHours">
      <Query>
        <DataSourceName>nochouse</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@USER">
            <Value>=Parameters!User.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @REPORTDATE DATE = DATEADD(MONTH, -6, GETDATE())

--daily NH ticket hours
SELECT
	TT.SqlUserID
	,US.FirstName + ' ' +  US.LastName [Name]
	,ROUND(CAST(SUM(CAST(DATEDIFF(SECOND, TT.StartTime, TT.EndTime) AS FLOAT) / 60 / 60) AS FLOAT),6) [TimeSheetHours]
	,SUM(DATEDIFF(MINUTE, TT.StartTime, TT.EndTime))/60 [Hours]
	,SUM(DATEDIFF(MINUTE, TT.StartTime, TT.EndTime)) % 60 [Minutes]
FROM
	Reporting.dbo.TimeTracking TT
	INNER JOIN CSYS.dbo.SYSA_DAT_Users US
		ON TT.SqlUserID = US.SqlUserId
WHERE
	US.WindowsUserName = REPLACE(@USER,'UHEAA\', '')
	AND TT.SqlUserID IN 
	(
		 3922
		,1492
		,1298
		,1198
		,1399
		,1801
		,1395
		,1686
		,1256
		,1152
		,1519
		,1518
		,1820
		,1818
		,1703
		,1268
		,1734
		,1570
		,1276
		,1161
		,1417
		,1550
		,1280
		,1415
		,3910
		,1451
		,1573
		,1624
		,1819
		,1685
	) 
	AND DAY(TT.EndTime) = DAY(@reportDate)
	AND MONTH(TT.EndTime) = MONTH(@reportDate)
	AND	YEAR(TT.EndTime) = YEAR(@reportDate)
GROUP BY
	TT.SqlUserID
	,US.FirstName
	,US.LastName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
