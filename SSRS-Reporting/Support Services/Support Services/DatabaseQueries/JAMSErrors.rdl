    <DataSource Name="nochouseJAMSV6">
      <DataSourceReference>/Data Sources/nochouseJAMSV6</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>3e88f6b3-4ef9-4ddb-8141-c720bc6ef359</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>nochouseJAMSV6</DataSourceName>
        <CommandText>SELECT 
		F.Folder_Name
		,J.Job_Name
		,CONVERT(DECIMAL(5,2),(J.[resubmit_delay]*100/3600))/100 [HourDelay]
,MAX(CAST( H.completion_time AS DATE)) [MaxDate]
		,MAX(DATEADD(hour, -6, H.completion_time)) [Last_Completion_Time]
		,COUNT(*) [ErrorCount]
		,'rallred@utahsbr.edu' [Email]
  FROM [Job] J
  INNER JOIN [Folder] F  ON F.folder_ID = J.folder_id
  INNER JOIN [Hist] H ON J.job_id = H.job_id
	
	WHERE j.job_id &lt;&gt; '186' AND H.finalsts != 0 AND J.auto_submit != 0 AND
	(J.[last_success] &lt; J.[last_error] AND H.[completion_time] &gt; J.[last_success] )
	AND F.folder_id IN ('8','10','15')
		
		GROUP BY F.Folder_Name,J.[job_name],CONVERT(DECIMAL(5,2),(J.[resubmit_delay]*100/3600))/100 ,J.[last_autosubmit]
			
			HAVING (COUNT(*) &gt; 1 OR CONVERT(DECIMAL(5,2),(J.[resubmit_delay]*100/3600))/100 = 0  AND MAX(CAST( H.completion_time AS DATE)) = CAST(GETDATE() AS DATE))

  ORDER BY F.folder_name, J.[job_name]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
