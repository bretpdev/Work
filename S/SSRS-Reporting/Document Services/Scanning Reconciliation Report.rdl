  <DataSets>
    <DataSet Name="ScanningReconciliationReport">
      <Query>
        <DataSourceName>KofaxCapture</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@reportDate">
            <Value>=Parameters!reportDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
		V.UserName AS UserID, 
		V.BatchDescription AS DocID,
		SUM(B.CompletedDocuments) AS CompletedDocuments
	FROM
		viewStatsSession V
		JOIN 
			(
				SELECT 
					BatchModuleID, 
					SUM(CompletedDocs) AS CompletedDocuments
				FROM 
					StatsFormType 
				GROUP BY 
					BatchModuleID
			)  B
			ON V.BatchModuleID = B.BatchModuleID
	WHERE 
		V.ModuleStartDateTime IS NOT NULL
		AND CAST(V.SessionEndDateTime AS DATE) = @reportDate
		AND V.ModuleName = 'Scan'
	GROUP BY 
		V.UserName,
		V.BatchDescription
	ORDER BY
		V.UserName,
		V.BatchDescription</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
