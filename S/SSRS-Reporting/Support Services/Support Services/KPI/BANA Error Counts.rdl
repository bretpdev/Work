  <DataSets>
    <DataSet Name="BANA_error_data">
      <Query>
        <DataSourceName>opsdev</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@errorType">
            <Value>=Parameters!errorType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	BEA.error_code,
	ISNULL(CEC.error_code_description, 'Unknown - Description needs to be added to CompassErrorCodes table.') [error_code_description],
	ISNULL(CEC.error_type, 'Unknown - Error type needs to be added to CompassErrorCodes table.') [error_type],
	COUNT(BEA.br_ssn) [error_count],
	CONVERT(VARCHAR(20), BEA.AddedAt, 110) + RIGHT(CONVERT(VARCHAR, BEA.AddedAt, 100), 8) [AddedAt]
FROM
	EA27_BANA.dbo.Borrower_Errors_All BEA
	LEFT JOIN EA27_BANA.dbo.CompassErrorCodes CEC ON CEC.error_code = BEA.error_code
WHERE
	BEA.AddedAt IS NOT NULL
	AND
	ISNULL(CEC.error_type, 'Unknown') in (@errorType)
GROUP BY
	BEA.error_code,
	CEC.error_code_description,
	CEC.error_type,
	CONVERT(VARCHAR(20), BEA.AddedAt, 110) + RIGHT(CONVERT(VARCHAR, BEA.AddedAt, 100), 8)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="error_code">
          <DataField>error_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="error_code_description">
          <DataField>error_code_description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="error_type">
          <DataField>error_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="error_count">
          <DataField>error_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AddedAt">
          <DataField>AddedAt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="error_types">
      <Query>
        <DataSourceName>opsdev</DataSourceName>
        <CommandText>SELECT DISTINCT error_type FROM EA27_BANA.dbo.CompassErrorCodes
UNION ALL
SELECT 'Unknown'</CommandText>
      </Query>
      <Fields>
        <Field Name="error_type">
          <DataField>error_type</DataField>
